<?xml version="1.0" encoding="UTF-8"?>
<datamodel name="Ethernet" version="2.0">
    <object name="Ethernet.Interface.{i}.">
        <parameter name="X_ADB_LastChange" access="readOnly" store="no">
            <syntax>
                <unsignedInt>
                    <units value="seconds"/>
                </unsignedInt>
                <default value="0"/>
            </syntax>
        </parameter>
    </object>
    <object name="Ethernet.Link.{i}.">
        <parameter name="X_ADB_LastChange" access="readOnly" store="no">
            <syntax>
                <unsignedInt>
                    <units value="seconds"/>
                </unsignedInt>
            </syntax>
        </parameter>
        <parameter name="X_ADB_Promisc" access="readWrite">
            <description>When set to {{true}}, it puts the interface into promiscuous mode.</description>
            <syntax>
                <boolean/>
                <default value="false"/>
            </syntax>
        </parameter>
    </object>
</datamodel>
