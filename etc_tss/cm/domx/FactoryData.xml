<?xml version="1.0" encoding="UTF-8"?>
<datamodel name="FactoryData" version="2.0">
    <object name="X_ADB_FactoryData." access="readOnly" excludeUser="CWMP" minEntries="1" maxEntries="1" store="no">
        <parameter name="BaseMACAddress" access="readWrite">
            <description>
                The base MAC address allocated to this CPE.
                Interface MAC addresses will be calculated from this, increasing by one for each used address.
            </description>
            <syntax>
                <string>
                    <size minLength="0" maxLength="17"/>
                    <pattern value=""/>
                    <pattern value="([0-9A-Fa-f][0-9A-Fa-f]:){5}([0-9A-Fa-f][0-9A-Fa-f])"/>
                </string>
                <default value="dc:0b:1a:bf:13:01"/>
            </syntax>
        </parameter>
        <parameter name="SerialNumber" access="readOnly">
            <syntax>
                <string>
                    <size minLength="0" maxLength="64"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="HardwareVersion" access="readOnly">
            <syntax>
                <string>
                    <size minLength="0" maxLength="64"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="ProductClass" access="readOnly">
            <syntax>
                <string>
                    <size minLength="0" maxLength="64"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="ModelName" access="readOnly">
            <syntax>
                <string>
                    <size minLength="0" maxLength="64"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="Manufacturer" access="readOnly">
            <syntax>
                <string>
                    <size minLength="0" maxLength="64"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="ManufacturerOUI" access="readOnly">
            <syntax>
                <string>
                    <size minLength="6" maxLength="6"/>
                    <pattern value="[0-9A-F]{6}"/>
                </string>
            </syntax>
        </parameter>
    </object>
</datamodel>
