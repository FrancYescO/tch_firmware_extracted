<?xml version="1.0" encoding="UTF-8"?>
<datamodel name="IP" version="2.0">
    <object name="IP.Diagnostics." access="readOnly" minEntries="1" maxEntries="1">
        <parameter name="X_ADB_IPPingNumberOfEntries" access="readOnly" activeNotify="canDeny" store="no">
            <description>{{numentries}}</description>
            <syntax>
                <unsignedInt/>
                <default value="0"/>
            </syntax>
        </parameter>
    </object>
    <object name="IP.Diagnostics.X_ADB_IPPing.{i}." access="readOnly" minEntries="0" maxEntries="unbounded" set="IPPing.sh" numEntriesParameter="X_ADB_IPPingNumberOfEntries">
        <description>This object provides access to an IP-layer ping test.
        The purpose is to run and store results for multiple ping tests.
        Each instance has the same usage and behavior as described for {{object|#.IPPing}}.
        </description>
        <parameter name="AverageResponseTime" access="readOnly" activeNotify="canDeny">
            <description>See description for {{param|#.IPPing.AverageResponseTime}}.</description>
            <syntax>
                <unsignedInt>
                    <units value="milliseconds"/>
                </unsignedInt>
            </syntax>
        </parameter>
        <parameter name="DSCP" access="readWrite">
            <description>See description for {{param|#.IPPing.DSCP}}.</description>
            <syntax>
                <unsignedInt>
                    <range minInclusive="0" maxInclusive="63" step="1"/>
                </unsignedInt>
            </syntax>
        </parameter>
        <parameter name="DataBlockSize" access="readWrite">
            <description>See description for {{param|#.IPPing.DataBlockSize}}.</description>
            <syntax>
                <unsignedInt>
                    <range minInclusive="1" maxInclusive="65535" step="1"/>
                </unsignedInt>
                <default value="64"/>
            </syntax>
        </parameter>
        <parameter name="DiagnosticsState" access="readWrite" activeNotify="canDeny" localNotify="forceEnabled">
            <description>See description for {{param|#.IPPing.DiagnosticsState}}.</description>
            <syntax>
                <string>
                    <enumeration value="None"/>
                    <enumeration value="Requested"/>
                    <enumeration value="Complete"/>
                    <enumeration value="Error_CannotResolveHostName"/>
                    <enumeration value="Error_Internal"/>
                    <enumeration value="Error_Other"/>
                </string>
                <default value="None"/>
            </syntax>
        </parameter>
        <parameter name="FailureCount" access="readOnly" activeNotify="canDeny">
            <description>See description for {{param|#.IPPing.FailureCount}}.</description>
            <syntax>
                <unsignedInt/>
            </syntax>
        </parameter>
        <parameter name="Host" access="readWrite">
            <description>See description for {{param|#.IPPing.Host}}.</description>
            <syntax>
                <string>
                    <size minLength="0" maxLength="256"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="HostReference" access="readWrite">
            <description>
            If defined, the hostname or IP address to ping is read from this reference parameter instead of the {{param|Host}} parameter, which is ignored.
            </description>
            <syntax>
                <string>
                    <size minLength="0" maxLength="256"/>
                    <pathRef refType="strong" targetType="parameter"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="Interface" access="readWrite">
            <description>See description for {{param|#.IPPing.Interface}}.</description>
            <syntax>
                <string>
                    <size minLength="0" maxLength="256"/>
                    <pathRef targetParent=".IP.Interface." refType="strong" targetType="row"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="MaximumResponseTime" access="readOnly" activeNotify="canDeny">
            <description>See description for {{param|#.IPPing.MaximumResponseTime}}.</description>
            <syntax>
                <unsignedInt>
                    <units value="milliseconds"/>
                </unsignedInt>
            </syntax>
        </parameter>
        <parameter name="MinimumResponseTime" access="readOnly" activeNotify="canDeny">
            <description>See description for {{param|#.IPPing.MinimumResponseTime}}.</description>
            <syntax>
                <unsignedInt>
                    <units value="milliseconds"/>
                </unsignedInt>
            </syntax>
        </parameter>
        <parameter name="NumberOfRepetitions" access="readWrite">
            <description>See description for {{param|#.IPPing.NumberOfRepetitions}}.</description>
            <syntax>
                <unsignedInt>
                    <range minInclusive="1" step="1"/>
                </unsignedInt>
                <default value="3"/>
            </syntax>
        </parameter>
        <parameter name="SuccessCount" access="readOnly" activeNotify="canDeny">
            <description>See description for {{param|#.IPPing.SuccessCount}}.</description>
            <syntax>
                <unsignedInt/>
            </syntax>
        </parameter>
        <parameter name="Timeout" access="readWrite">
            <description>See description for {{param|#.IPPing.Timeout}}.</description>
            <syntax>
                <unsignedInt>
                    <range minInclusive="1" step="1"/>
                    <units value="milliseconds"/>
                </unsignedInt>
                <default value="5000"/>
            </syntax>
        </parameter>
    </object>
    <object name="IP.Diagnostics.IPPing." access="readOnly" minEntries="1" maxEntries="1" store="no" set="IPPing.sh">
        <parameter name="X_ADB_StartTime" access="readOnly">
            <description>
            After the test is requested, this parameter contains the absolute start time of the IP ping.
            </description>
            <syntax>
                <dateTime/>
                <default value="0001-01-01T00:00:00Z"/>
            </syntax>
        </parameter>
        <parameter name="X_ADB_TestCode" access="readWrite">
            <description>
            A test code string that will be copied in the uploaded report files.
            </description>
            <syntax>
                <string>
                    <size maxLength="64"/>
                </string>
            </syntax>
        </parameter>
    </object>
    <object name="IP.Diagnostics.DownloadDiagnostics." access="readOnly" minEntries="1" maxEntries="1" store="no" set="DownloadUploadDiagnostics.sh">
        <parameter name="X_ADB_TestCode" access="readWrite">
            <description>
            A test code string that will be copied in the uploaded report files.
            </description>
            <syntax>
                <string>
                    <size maxLength="64"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="X_ADB_TrafficPriority" access="readWrite" supported="no">
            <description>Set the upstream priority queue where the traffic must be routed.</description>
            <syntax>
                <string>
                    <enumeration value="None"/>
                    <enumeration value="Background"/>
                    <enumeration value="Foreground"/>
                </string>
                <default value="None"/>
            </syntax>
        </parameter>
        <parameter name="X_ADB_TestRate" access="readOnly">
            <description>The test data transfer rate, in {{units}}, as from:
: 8 * {{param|TestBytesReceived}} / ({{param|EOMTime}} - {{param|BOMTime}}) / 1000
            </description>
            <syntax>
                <unsignedInt>
                    <units value="Kbps"/>
                </unsignedInt>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="X_ADB_TotalRate" access="readOnly">
            <description>The total data transfer rate, in {{units}}, as from:
: 8 * {{param|TotalBytesReceived}} / ({{param|EOMTime}} - {{param|BOMTime}}) / 1000
            </description>
            <syntax>
                <unsignedInt>
                    <units value="Kbps"/>
                </unsignedInt>
                <default value="0"/>
            </syntax>
        </parameter>
    </object>
    <object name="IP.Diagnostics.UploadDiagnostics." access="readOnly" minEntries="1" maxEntries="1" store="no" set="DownloadUploadDiagnostics.sh">
        <parameter name="X_ADB_TestCode" access="readWrite">
            <description>
            A test code string that will be copied in the uploaded report files.
            </description>
            <syntax>
                <string>
                    <size maxLength="64"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="X_ADB_TrafficPriority" access="readWrite" supported="no">
            <description>Set the upstream priority queue where the traffic must be routed.</description>
            <syntax>
                <string>
                    <enumeration value="None"/>
                    <enumeration value="Background"/>
                    <enumeration value="Foreground"/>
                </string>
                <default value="None"/>
            </syntax>
        </parameter>
        <parameter name="X_ADB_TestRate" access="readOnly">
            <description>The test data transfer rate, in {{units}}, as from:
: 8 * {{param|TestBytesSent}} / ({{param|EOMTime}} - {{param|BOMTime}}) / 1000
            </description>
            <syntax>
                <unsignedInt>
                    <units value="Kbps"/>
                </unsignedInt>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="X_ADB_TotalRate" access="readOnly">
            <description>The total data transfer rate, in {{units}}, as from:
: 8 * {{param|TotalBytesSent}} / ({{param|EOMTime}} - {{param|BOMTime}}) / 1000
            </description>
            <syntax>
                <unsignedInt>
                    <units value="Kbps"/>
                </unsignedInt>
                <default value="0"/>
            </syntax>
        </parameter>
    </object>
</datamodel>
