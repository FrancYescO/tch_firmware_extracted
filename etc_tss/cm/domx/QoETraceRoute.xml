<?xml version="1.0" encoding="UTF-8"?>
<datamodel name="QoEDiagnostics" version="2.0">
  <object name="IP.Diagnostics.">
    <parameter name="X_ADB_TraceRouteNumberOfEntries" access="readOnly" store="no">
        <description>{{numentries}}</description>
        <syntax>
            <unsignedInt/>
            <default value="0"/>
        </syntax>
    </parameter>
  </object>
  <object name="IP.Diagnostics.X_ADB_TraceRoute." access="readOnly" minEntries="1" maxEntries="unbounded" store="no" numEntriesParameter="X_ADB_TraceRouteNumberOfEntries" set="Traceroute.sh">
    <parameter name="Alias" access="readOnly" key="yes">
        <description>Name of this object instance.</description>
        <syntax>
            <dataType ref="Alias"/>
        </syntax>
    </parameter>
    <parameter name="X_ADB_StartTime" access="readOnly">
        <description>
        After the test is requested, this parameter contains the absolute start time of the IP ping.
        </description>
        <syntax>
            <dateTime/>
            <default value="0001-01-01T00:00:00Z"/>
        </syntax>
    </parameter>
    <parameter name="DSCP" access="readWrite">
        <syntax>
            <unsignedInt>
                <range minInclusive="0" maxInclusive="63" step="1"/>
            </unsignedInt>
            <default value="0"/>
        </syntax>
    </parameter>
    <parameter name="DataBlockSize" access="readWrite">
        <syntax>
            <unsignedInt>
                <range minInclusive="1" maxInclusive="65535" step="1"/>
            </unsignedInt>
            <default value="38"/>
        </syntax>
    </parameter>
    <parameter name="DiagnosticsState" access="readWrite" activeNotify="canDeny" localNotify="forceEnabled">
        <syntax>
            <string>
                <enumeration value="None"/>
                <enumeration value="Requested"/>
                <enumeration value="Complete"/>
                <enumeration value="Error_CannotResolveHostName"/>
                <enumeration value="Error_MaxHopCountExceeded"/>
                <enumeration value="Error_Internal"/>
                <enumeration value="Error_Other"/>
                <enumeration value="Error_OutOfMemory"/>
                <enumeration value="Error_CPUThresholdExceeded"/>
                <enumeration value="Error_TrafficThresholdExceeded"/>
            </string>
        </syntax>
    </parameter>
    <parameter name="Host" access="readWrite">
        <syntax>
            <string>
                <size minLength="0" maxLength="256"/>
            </string>
        </syntax>
    </parameter>
    <parameter name="Interface" access="readWrite">
        <syntax>
            <string>
                <size minLength="0" maxLength="256"/>
                <pathRef targetParent=".IP.Interface." refType="strong" targetType="row"/>
            </string>
        </syntax>
    </parameter>
    <parameter name="MaxHopCount" access="readWrite">
        <syntax>
            <unsignedInt>
                <range minInclusive="1" maxInclusive="64" step="1"/>
            </unsignedInt>
            <default value="64" />
        </syntax>
    </parameter>
    <parameter name="NumberOfTries" access="readWrite">
        <syntax>
            <unsignedInt>
                <range minInclusive="1" maxInclusive="3" step="1"/>
            </unsignedInt>
            <default value="3"/>
        </syntax>
    </parameter>
    <parameter name="ResponseTime" access="readOnly" activeNotify="canDeny">
        <syntax>
            <unsignedInt>
                <units value="milliseconds"/>
            </unsignedInt>
        </syntax>
    </parameter>
    <parameter name="RouteHopsNumberOfEntries" access="readOnly" activeNotify="canDeny" store="no">
        <syntax>
            <unsignedInt/>
            <default value="0"/>
        </syntax>
    </parameter>
    <parameter name="Timeout" access="readWrite">
        <syntax>
            <unsignedInt>
                <range minInclusive="1" step="1"/>
                <units value="milliseconds"/>
            </unsignedInt>
            <default value="5000"/>
        </syntax>
    </parameter>
    <parameter name="ProtocolVersion" access="readWrite">
        <syntax>
            <string>
                <enumeration value="Any"/>
                <enumeration value="IPv4"/>
                <enumeration value="IPv6"/>
            </string>
            <default value="Any"/>
        </syntax>
    </parameter>
  </object>
  <object name="IP.Diagnostics.X_ADB_TraceRoute.RouteHops.{i}." access="readOnly" minEntries="0" maxEntries="unbounded" numEntriesParameter="RouteHopsNumberOfEntries" store="no">
      <parameter name="ErrorCode" access="readOnly" activeNotify="canDeny">
          <syntax>
              <unsignedInt/>
          </syntax>
      </parameter>
      <parameter name="Host" access="readOnly" activeNotify="canDeny">
          <syntax>
              <string>
                  <size minLength="0" maxLength="256"/>
              </string>
          </syntax>
      </parameter>
      <parameter name="HostAddress" access="readOnly" activeNotify="canDeny">
          <syntax>
              <string/>
          </syntax>
      </parameter>
      <parameter name="RTTimes" access="readOnly" activeNotify="canDeny">
          <syntax>
              <list>
                  <size minLength="0" maxLength="16"/>
              </list>
              <unsignedInt>
                  <units value="milliseconds"/>
              </unsignedInt>
          </syntax>
      </parameter>
  </object>
  <object name="X_ADB_QoE.TraceRouteDiagnostics.">
    <parameter name="DSCP" access="readWrite">
        <syntax>
            <unsignedInt>
                <range minInclusive="0" maxInclusive="63" step="1"/>
            </unsignedInt>
            <default value="0"/>
        </syntax>
    </parameter>
    <parameter name="DataBlockSize" access="readWrite">
        <syntax>
            <unsignedInt>
                <range minInclusive="1" maxInclusive="65535" step="1"/>
            </unsignedInt>
            <default value="38"/>
        </syntax>
    </parameter>
    <parameter name="Host" access="readWrite">
        <syntax>
            <string>
                <size minLength="0" maxLength="256"/>
            </string>
        </syntax>
    </parameter>
    <parameter name="Interface" access="readWrite">
        <syntax>
            <string>
                <size minLength="0" maxLength="256"/>
                <pathRef targetParent=".IP.Interface." refType="strong" targetType="row"/>
            </string>
        </syntax>
    </parameter>
    <parameter name="MaxHopCount" access="readWrite">
        <syntax>
            <unsignedInt>
                <range minInclusive="1" maxInclusive="64" step="1"/>
            </unsignedInt>
            <default value="64" />
        </syntax>
    </parameter>
    <parameter name="NumberOfTries" access="readWrite">
        <syntax>
            <unsignedInt>
                <range minInclusive="1" maxInclusive="3" step="1"/>
            </unsignedInt>
            <default value="3"/>
        </syntax>
    </parameter>
    <parameter name="Timeout" access="readWrite">
        <syntax>
            <unsignedInt>
                <range minInclusive="1" step="1"/>
                <units value="milliseconds"/>
            </unsignedInt>
            <default value="5000"/>
        </syntax>
    </parameter>
    <parameter name="ProtocolVersion" access="readWrite">
        <syntax>
            <string>
                <enumeration value="Any"/>
                <enumeration value="IPv4"/>
                <enumeration value="IPv6"/>
            </string>
            <default value="Any"/>
        </syntax>
    </parameter>
  </object>
</datamodel>
