<?xml version="1.0" encoding="UTF-8"?>
<datamodel name="WiFi" version="2.0">
    <object name="WiFi.AccessPoint.{i}.AssociatedDevice.{i}." del="QoEWiFi.sh">
        <parameter name="MACAddress" set="QoEWiFi.sh"/>
    </object>
    <object name="WiFi.AccessPoint.{i}.X_ADB_QoE." access="readWrite" minEntries="1" maxEntries="1" set="QoEWiFi.sh">
        <description>Object to support Quality of Experience diagnostics.</description>
        <parameter name="SamplingInterval">
            <description>Sampling interval in {{units}}. 0 mean disabled diagnostics.</description>
            <syntax>
                <unsignedInt>
                    <units value="seconds"/>
                </unsignedInt>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="SamplesCount">
            <description>Max number of samples.</description>
            <syntax>
                <unsignedInt/>
                <default value="100"/>
            </syntax>
        </parameter>
        <parameter name="Update" access="readOnly">
            <description>Force update of X_ADB_QoE diagnostic for current AccessPoint.</description>
            <syntax>
                <boolean/>
                <default value="false"/>
            </syntax>
        </parameter>
        <parameter name="ResetStats" access="readOnly">
            <description>Reset X_ADB_QoE diagnostics for current AccessPoint.</description>
            <syntax>
                <boolean/>
                <default value="false"/>
            </syntax>
        </parameter>
        <parameter name="NodeNumberOfEntries" access="readOnly">
            <description>{{numentries}}</description>
            <syntax>
                <unsignedInt/>
            </syntax>
        </parameter>
    </object>
    <object name="WiFi.AccessPoint.{i}.X_ADB_QoE.Node.{i}." access="readOnly" minEntries="0" maxEntries="unbounded" numEntriesParameter="NodeNumberOfEntries">
        <description>
        Each instance represents a set of measurements performed on the link between the AP and a given STA.
        </description>
        <parameter name="Active" access="readOnly">
            <description>Status of node (STA). True mean that is associated and active</description>
            <syntax>
                <boolean/>
                <default value="false"/>
            </syntax>
        </parameter>
        <parameter name="MACAddress" access="readOnly" key="yes">
            <description>MAC address of node (STA).</description>
            <syntax>
                <string>
                    <size minLength="0" maxLength="17"/>
                    <pattern value=""/>
                    <pattern value="([0-9A-Fa-f][0-9A-Fa-f]:){5}([0-9A-Fa-f][0-9A-Fa-f])"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="LastAssociation" access="readOnly">
            <description>Last association time.</description>
            <syntax>
                <dateTime/>
            </syntax>
        </parameter>
        <parameter name="LastDisassociation" access="readOnly">
            <description>Last disassociation time.</description>
            <syntax>
                <dateTime/>
            </syntax>
        </parameter>
        <parameter name="RSSI" access="readOnly">
            <description>RSSI (signal strength) on last {{param|#.SamplingInterval}}.</description>
            <syntax>
                <int>
                    <range minInclusive="-200" maxInclusive="0" step="1"/>
                    <units value="dBm"/>
                </int>
            </syntax>
        </parameter>
        <parameter name="RSSIList" access="readOnly">
            <description>List of {{param|RSSI}} saved every {{param|#.SamplingInterval}}.</description>
            <syntax>
                <list/>
                <int>
                    <range minInclusive="-200" maxInclusive="0" step="1"/>
                    <units value="dBm"/>
                </int>
            </syntax>
        </parameter>
        <parameter name="Throughput" access="readOnly">
            <description>Estimated throughput on last {{param|#.SamplingInterval}}.</description>
            <syntax>
                <unsignedInt>
                    <units value="kbps"/>
                </unsignedInt>
            </syntax>
        </parameter>
        <parameter name="ThroughputList" access="readOnly">
            <description>List of {{param|Throughput}} saved every {{param|#.SamplingInterval}}.</description>
            <syntax>
                <list/>
                <unsignedInt>
                    <units value="kbps"/>
                </unsignedInt>
            </syntax>
        </parameter>
        <parameter name="BytesSent" access="readOnly" activeNotify="canDeny" store="no">
            <description>The total number of bytes sent to STA on last {{param|#.SamplingInterval}}.</description>
            <syntax>
                <unsignedLong/>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="BytesReceived" access="readOnly" activeNotify="canDeny" store="no">
            <description>The total number of bytes received from STA on last {{param|#.SamplingInterval}}.</description>
            <syntax>
                <unsignedLong/>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="UserThroughput" access="readOnly" activeNotify="canDeny">
            <description>The real througput (sum of incoming and outcoming) of current STA.</description>
            <syntax>
                <unsignedInt>
                    <units value="kbps"/>
                </unsignedInt>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="UserThroughputList" access="readOnly" activeNotify="canDeny">
            <description>List of {{param|UserThroughput}} saved every {{param|#.SamplingInterval}}.</description>
            <syntax>
                <list/>
                <unsignedInt>
                    <units value="kbps"/>
                </unsignedInt>
            </syntax>
        </parameter>
    </object>
</datamodel>
