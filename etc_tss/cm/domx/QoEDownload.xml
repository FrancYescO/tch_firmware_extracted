<?xml version="1.0" encoding="UTF-8"?>
<datamodel name="QoEDiagnostics" version="2.0">
    <object name="IP.Diagnostics.">
        <parameter name="X_ADB_DownloadDiagnosticsNumberOfEntries" access="readOnly" store="no">
            <description>{{numentries}}</description>
            <syntax>
                <unsignedInt/>
                <default value="0"/>
            </syntax>
        </parameter>
    </object>
    <object name="IP.Diagnostics.X_ADB_DownloadDiagnostics.{i}." access="readOnly" minEntries="1" maxEntries="unbounded" numEntriesParameter="X_ADB_DownloadDiagnosticsNumberOfEntries" set="DownloadUploadDiagnostics.sh">
        <description>This object defines the diagnostics configuration for a HTTP and FTP DownloadDiagnostics test.
        The purpose is to run and store results for multiple download tests.
        Each instance has the same usage and behavior as described for {{object|#.DownloadDiagnostics}}.
        </description>
        <parameter name="Alias" access="readOnly" key="yes">
            <description>Name of this object instance.</description>
            <syntax>
                <dataType ref="Alias"/>
            </syntax>
        </parameter>
        <parameter name="DiagnosticsState" access="readWrite">
            <description>{{enum}}
            See description in {{param|#.DownloadDiagnostics.DiagnosticsState}}.
            </description>
            <syntax>
                <string>
                    <enumeration value="None"/>
                    <enumeration value="Requested"/>
                    <enumeration value="Completed"/>
                    <enumeration value="Error_InitConnectionFailed"/>
                    <enumeration value="Error_NoResponse"/>
                    <enumeration value="Error_TransferFailed"/>
                    <enumeration value="Error_PasswordRequestFailed"/>
                    <enumeration value="Error_LoginFailed"/>
                    <enumeration value="Error_NoTransferMode"/>
                    <enumeration value="Error_NoPASV"/>
                    <enumeration value="Error_IncorrectSize"/>
                    <enumeration value="Error_Timeout"/>
                    <enumeration value="Error_NoCWD"/>
                </string>
                <default value="None"/>
            </syntax>
        </parameter>
        <parameter name="Interface" access="readWrite">
            <description>{{reference}}
            See description in {{param|#.DownloadDiagnostics.Interface}}.
            </description>
            <syntax>
                <string>
                    <size minLength="0" maxLength="256"/>
                    <pathRef targetParent=".IP.Interface." refType="strong" targetType="row"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="DownloadURL" access="readWrite">
            <description>
            See description in {{param|#.DownloadDiagnostics.DownloadURL}}.
            </description>
            <syntax>
                <string>
                    <size minLength="0" maxLength="256"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="DownloadTransports" access="readOnly">
            <description>{{enum}}
            See description in {{param|#.DownloadDiagnostics.DownloadTransports}}.
            </description>
            <syntax>
                <string>
                    <enumeration value="HTTP"/>
                    <enumeration value="FTP"/>
                </string>
                <default value="HTTP,FTP"/>
            </syntax>
        </parameter>
        <parameter name="DSCP" access="readWrite">
            <description>
            See description in {{param|#.DownloadDiagnostics.DSCP}}.
            </description>
            <syntax>
                <unsignedInt>
                    <range minInclusive="0" maxInclusive="63" step="1"/>
                </unsignedInt>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="EthernetPriority" access="readWrite">
            <description>
            See description in {{param|#.DownloadDiagnostics.EthernetPriority}}.
            </description>
            <syntax>
                <unsignedInt>
                    <range minInclusive="0" maxInclusive="7" step="1"/>
                </unsignedInt>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="ROMTime" access="readOnly" activeNotify="canDeny">
            <description>
            See description in {{param|#.DownloadDiagnostics.ROMTime}}.
            </description>
            <syntax>
                <dateTime/>
            </syntax>
        </parameter>
        <parameter name="BOMTime" access="readOnly" activeNotify="canDeny">
            <description>
            See description in {{param|#.DownloadDiagnostics.BOMTime}}.
            </description>
            <syntax>
                <dateTime/>
            </syntax>
        </parameter>
        <parameter name="EOMTime" access="readOnly" activeNotify="canDeny">
            <description>
            See description in {{param|#.DownloadDiagnostics.EOMTime}}.
            </description>
            <syntax>
                <dateTime/>
            </syntax>
        </parameter>
        <parameter name="TestBytesReceived" access="readOnly" activeNotify="canDeny">
            <description>
            See description in {{param|#.DownloadDiagnostics.TestBytesReceived}}.
            </description>
            <syntax>
                <unsignedInt/>
            </syntax>
        </parameter>
        <parameter name="TotalBytesReceived" access="readOnly" activeNotify="canDeny">
            <description>
            See description in {{param|#.DownloadDiagnostics.TotalBytesReceived}}.
            </description>
            <syntax>
                <unsignedInt/>
            </syntax>
        </parameter>
        <parameter name="TCPOpenRequestTime" access="readOnly" activeNotify="canDeny">
            <description>
            See description in {{param|#.DownloadDiagnostics.TCPOpenRequestTime}}.
            </description>
            <syntax>
                <dateTime/>
            </syntax>
        </parameter>
        <parameter name="TCPOpenResponseTime" access="readOnly" activeNotify="canDeny">
            <description>
            See description in {{param|#.DownloadDiagnostics.TCPOpenResponseTime}}.
            </description>
            <syntax>
                <dateTime/>
            </syntax>
        </parameter>
        <parameter name="TimeBasedTestDuration" access="readWrite">
            <description>
            See description in {{param|#.DownloadDiagnostics.TimeBasedTestDuration}}.
            </description>
            <syntax>
                <unsignedInt>
                    <range minInclusive="0" maxInclusive="999" step="1"/>
                    <units value="seconds"/>
                </unsignedInt>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="TimeBasedTestIncrements" access="readWrite">
            <description>
            See description in {{param|#.DownloadDiagnostics.TimeBasedTestIncrements}}.
            </description>
            <syntax>
                <unsignedInt>
                    <units value="seconds"/>
                </unsignedInt>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="TimeBasedTestIncrementsOffset" access="readWrite">
            <description>
            See description in {{param|#.DownloadDiagnostics.TimeBasedTestIncrementsOffset}}.
            </description>
            <syntax>
                <unsignedInt>
                    <units value="seconds"/>
                </unsignedInt>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="X_ADB_TrafficPriority" access="readWrite" supported="no">
            <description>See description in {{param|#.DownloadDiagnostics.X_ADB_TrafficPriority}}.</description>
            <syntax>
                <string>
                    <enumeration value="None"/>
                    <enumeration value="Background"/>
                    <enumeration value="Foreground"/>
                </string>
                <default value="None"/>
            </syntax>
        </parameter>
        <parameter name="X_ADB_TestRate" access="readOnly">
            <description>
            See description in {{param|#.DownloadDiagnostics.X_ADB_TestRate}}.
            </description>
            <syntax>
                <unsignedInt>
                    <units value="Kbps"/>
                </unsignedInt>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="X_ADB_TotalRate" access="readOnly">
            <description>
            See description in {{param|#.DownloadDiagnostics.X_ADB_TotalRate}}.
            </description>
            <syntax>
                <unsignedInt>
                    <units value="Kbps"/>
                </unsignedInt>
                <default value="0"/>
            </syntax>
        </parameter>
    </object>
    <object name="IP.Diagnostics.X_ADB_DownloadDiagnostics.{i}.IncrementalResults.{i}." access="readOnly" minEntries="0" maxEntries="unbounded">
        <description>
        See description in {{object|##.DownloadDiagnostics.IncrementalResults}}.
        </description>
        <parameter name="IncTestBytesReceived" access="readOnly" activeNotify="canDeny">
            <description>
            See description in {{param|##.DownloadDiagnostics.IncrementalResults.{i}.IncTestBytesReceived}}.
            </description>
            <syntax>
                <unsignedInt/>
            </syntax>
        </parameter>
        <parameter name="IncBOMTime" access="readOnly" activeNotify="canDeny">
            <description>
            See description in {{param|##.DownloadDiagnostics.IncrementalResults.{i}.IncBOMTime}}.
            </description>
            <syntax>
                <dateTime/>
            </syntax>
        </parameter>
        <parameter name="IncEOMTime" access="readOnly" activeNotify="canDeny">
            <description>
            See description in {{param|##.DownloadDiagnostics.IncrementalResults.{i}.IncEOMTime}}.
            </description>
            <syntax>
                <dateTime/>
            </syntax>
        </parameter>
    </object>
    <object name="X_ADB_QoE.DownloadDiagnostics.">
        <parameter name="Interface" access="readWrite">
            <description>
            This value will be configured in the {{param|.IP.Diagnostics.X_ADB_DownloadDiagnostics.{i}.Interface}} parameter of the instances that will be created during this test campaign.
            </description>
            <syntax>
                <string>
                    <size minLength="0" maxLength="256"/>
                    <pathRef targetParent=".IP.Interface." refType="strong" targetType="row"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="DownloadURL" access="readWrite">
            <description>
            This value will be configured in the {{param|.IP.Diagnostics.X_ADB_DownloadDiagnostics.{i}.DownloadURL}} parameter of the instances that will be created during this test campaign.
            </description>
            <syntax>
                <string>
                    <size minLength="0" maxLength="256"/>
                    <pattern value="http://.+"/>
                    <pattern value="ftp://.+"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="TimeBasedTestDuration" access="readWrite">
            <description>
            This value will be configured in the {{param|.IP.Diagnostics.X_ADB_DownloadDiagnostics.{i}.TimeBasedTestDuration}} parameter of the instances that will be created during this test campaign.
            </description>
            <syntax>
                <unsignedInt>
                    <range minInclusive="0" maxInclusive="999" step="1"/>
                    <units value="seconds"/>
                </unsignedInt>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="TimeBasedTestIncrements" access="readWrite">
            <description>
            This value will be configured in the {{param|.IP.Diagnostics.X_ADB_DownloadDiagnostics.{i}.TimeBasedTestIncrements}} parameter of the instances that will be created during this test campaign.
            </description>
            <syntax>
                <unsignedInt>
                    <units value="seconds"/>
                </unsignedInt>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="TimeBasedTestIncrementsOffset" access="readWrite">
            <description>
            This value will be configured in the {{param|.IP.Diagnostics.X_ADB_DownloadDiagnostics.{i}.TimeBasedTestIncrementsOffset}} parameter of the instances that will be created during this test campaign.
            </description>
            <syntax>
                <unsignedInt>
                    <units value="seconds"/>
                </unsignedInt>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="DSCP" access="readWrite">
            <description>
            This value will be configured in the {{param|.IP.Diagnostics.X_ADB_DownloadDiagnostics.{i}.DSCP}} parameter of the instances that will be created during this test campaign.
            </description>
            <syntax>
                <unsignedInt>
                    <range minInclusive="0" maxInclusive="63" step="1"/>
                </unsignedInt>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="EthernetPriority" access="readWrite">
            <description>
            This value will be configured in the {{param|.IP.Diagnostics.X_ADB_DownloadDiagnostics.{i}.EthernetPriority}} parameter of the instances that will be created during this test campaign.
            </description>
            <syntax>
                <unsignedInt>
                    <range minInclusive="0" maxInclusive="7" step="1"/>
                </unsignedInt>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="TrafficPriority" access="readWrite">
            <description>This value will be configured in the {{param|.IP.Diagnostics.X_ADB_DownloadDiagnostics.{i}.X_ADB_TrafficPriority}} parameter of the instances that will be created during this test campaign.</description>
            <syntax>
                <string>
                    <enumeration value="None"/>
                    <enumeration value="Background"/>
                    <enumeration value="Foreground"/>
                </string>
                <default value="None"/>
            </syntax>
        </parameter>
    </object>
</datamodel>
