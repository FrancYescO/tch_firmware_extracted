<?xml version="1.0" encoding="UTF-8"?>
<datamodel name="QoEDiagnostics" version="2.0">
    <object name="X_ADB_QoE.LANDiagnostics.">
        <parameter name="Interface" access="readWrite">
            <syntax>
                <string>
                    <pathRef targetParent=".IP.Interface." refType="weak" targetType="row"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="Target">
            <syntax>
                <string>
                    <dataType ref="IPv4Address"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="ProbingMethod">
            <syntax>
                <string>
                    <enumeration value="ICMP"/>
                    <enumeration value="UDP"/>
                </string>
                <default value="ICMP"/>
            </syntax>
        </parameter>
        <parameter name="SmallMtu">
            <syntax>
                <unsignedInt>
                    <range minInclusive="0" maxInclusive="1514" step="1"/>
                </unsignedInt>
                <default value="1514"/>
            </syntax>
        </parameter>
        <parameter name="BigMtu">
            <syntax>
                <unsignedInt>
                    <range minInclusive="1514" maxInclusive="3028" step="1"/>
                </unsignedInt>
                <default value="3028"/>
            </syntax>
        </parameter>
        <parameter name="Numprobes">
            <syntax>
                <unsignedInt>
                    <range minInclusive="50" maxInclusive="10000" step="1"/>
                </unsignedInt>
                <default value="50"/>
            </syntax>
        </parameter>
        <parameter name="Interval">
            <syntax>
                <range minInclusive="50" maxInclusive="999" step="1"/>
                <units value="seconds"/>
                <default value="50"/>
            </syntax>
        </parameter>
        <parameter name="LowerPercentile">
            <syntax>
                <unsignedInt/>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="AvgPercentile">
            <syntax>
                <unsignedInt/>
                <default value="70"/>
            </syntax>
        </parameter>
        <parameter name="Timeout">
            <syntax>
                <unsignedInt>
                    <range minInclusive="30" maxInclusive="240" step="1"/>
                    <units value="seconds"/>
                </unsignedInt>
                <default value="180"/>
            </syntax>
        </parameter>
    </object>
</datamodel>
