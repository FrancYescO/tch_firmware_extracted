<?xml version="1.0" encoding="UTF-8"?>
<datamodel name="QoEDiagnostics" version="2.0">
    <object name="IP.Diagnostics.X_ADB_YoutubeDiagnostics.{i}.">
        <parameter name="DiagnosticsState" access="readWrite">
            <description>{{enum}}
            See description in {{param|#.DownloadDiagnostics.DiagnosticsState}}.
            </description>
            <syntax>
                <string>
                    <enumeration value="None"/>
                    <enumeration value="Requested"/>
                    <enumeration value="Completed"/>
                    <enumeration value="Error_TestFail"/>
                    <enumeration value="Error_NoURL"/>
                    <enumeration value="Error_OutOfMemory"/>
                    <enumeration value="Error_CPUThresholdExceeded"/>
                    <enumeration value="Error_TrafficThresholdExceeded"/>
                </string>
                <default value="None"/>
            </syntax>
        </parameter>
        <parameter name="StartTime" access="readOnly">
            <description>
            After the test is requested, this parameter contains the absolute start time of the IP ping.
            </description>
            <syntax>
                <dateTime/>
                <default value="0001-01-01T00:00:00Z"/>
            </syntax>
        </parameter>
        <parameter name="X_ADB_TestCode" access="readWrite">
            <description>
            A test code string that will be copied in the uploaded report files.
            </description>
            <syntax>
                <string>
                    <size maxLength="64"/>
                </string>
            </syntax>
        </parameter>
    </object>
    <object name="X_ADB_QoE.YoutubeDiagnostics.">
        <parameter name="URL" access="readWrite">
            <description>
                This value will be configured in the {{param|.IP.Diagnostics.X_ADB_YoutubeDiagnostics.{i}.DownloadURL}} parameter of the instances that will be created during this test campaign.
            </description>
            <syntax>
                <string>
                    <size minLength="0" maxLength="256"/>
                    <pattern value="http://.+"/>
                    <pattern value="https://.+"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="ProtocolVersion" access="readWrite">
            <description>
                This value if other than any, force ip protocol version during test.
            </description>
            <syntax>
                <string>
                    <enumeration value="Any"/>
                    <enumeration value="IPv4"/>
                    <enumeration value="IPv6"/>
                </string>
                <default value="Any"/>
            </syntax>
        </parameter>
        <parameter name="BufferSize" access="readWrite">
            <description>
                Length of playout buffer [sec]
            </description>
            <syntax>
                <unsignedInt/>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="MinTime" access="readWrite">
            <description>
                Test is considered failed if it terminates before this time [sec]
            </description>
            <syntax>
                <unsignedInt/>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="MaxTime" access="readWrite">
            <description>
                Finish test if takes more then this time [sec]
            </description>
            <syntax>
                <unsignedInt/>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="BitRate" access="readWrite">
            <description>
                If greater than 0, the Test Agent will attempt to download test stream at this fixed bitrate [bps]
            </description>
            <syntax>
                <unsignedInt/>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="OneBitRate" access="readWrite">
            <description>
                When set to true, the client does not switch to lower bit rate when a stall occurs
            </description>
            <syntax>
              <boolean/>
              <default value="false"/>
           </syntax>
        </parameter>
    </object>
</datamodel>
