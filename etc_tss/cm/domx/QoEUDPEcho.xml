<?xml version="1.0" encoding="UTF-8"?>
<datamodel name="QoEDiagnostics" version="2.0">
    <object name="IP.Diagnostics.">
        <parameter name="X_ADB_UDPEchoDiagnosticsNumberOfEntries" access="readOnly" store="no">
            <description>{{numentries}}</description>
            <syntax>
                <unsignedInt/>
                <default value="0"/>
            </syntax>
        </parameter>
    </object>
    <object name="IP.Diagnostics.X_ADB_UDPEchoDiagnostics.{i}." access="readOnly" minEntries="1" maxEntries="unbounded" numEntriesParameter="X_ADB_UDPEchoDiagnosticsNumberOfEntries" set="UDPEchoDiagnostics.sh">
        <description>This object defines the diagnostics configuration for UDPEcho diagnostics test.
        The purpose is to run and store results for multiple echo tests.
        Each instance has the same usage and behavior as described for {{object|#.UDPEchoDiagnostics}}.
        </description>
        <parameter name="Alias" access="readOnly" key="yes">
            <description>Name of this object instance.</description>
            <syntax>
                <dataType ref="Alias"/>
            </syntax>
        </parameter>
        <parameter name="DiagnosticsState" access="readWrite" activeNotify="canDeny" localNotify="forceEnabled">
            <description>{{enum}}
            See description in {{param|#.UDPEchoDiagnostics.DiagnosticsState}}.
            </description>
            <syntax>
              <string>
                 <enumeration value="None"/>
                 <enumeration value="Requested"/>
                 <enumeration value="Completed"/>
                 <enumeration value="Error_CannotResolveHostName"/>
                 <enumeration value="Error_Internal"/>
                 <enumeration value="Error_Other"/>
                 <enumeration value="Error_OutOfMemory"/>
                 <enumeration value="Error_CPUThresholdExceeded"/>
                 <enumeration value="Error_TrafficThresholdExceeded"/>
              </string>
              <default value="None"/>
           </syntax>
        </parameter>
        <parameter name="Interface" access="readWrite">
            <description>{{reference}}
            See description in {{param|#.UDPEchoDiagnostics.Interface}}.
            </description>
            <syntax>
              <string>
                 <size minLength="0" maxLength="256"/>
                 <pathRef targetParent=".IP.Interface." refType="weak" targetType="row"/>
              </string>
            </syntax>
        </parameter>
        <parameter name="EchoPlusEnabled" access="readWrite">
            <description>
            See description in {{param|#.UDPEchoDiagnostics.EchoPlusEnabled}}.
            </description>
            <syntax>
              <boolean/>
              <default value="false"/>
           </syntax>
        </parameter>
        <parameter name="Host" access="readWrite">
            <description>
            See description in {{param|#.UDPEchoDiagnostics.Host}}.
            </description>
            <syntax>
              <string>
                 <size minLength="0" maxLength="256"/>
              </string>
           </syntax>
        </parameter>
        <parameter name="X_ADB_UDPPort" access="readWrite">
           <description>The UDP port used for this service.</description>
           <syntax>
              <unsignedInt>
                  <range minInclusive="1" maxInclusive="65535"/>
              </unsignedInt>
              <default value="7"/>
           </syntax>
        </parameter>
        <parameter name="NumberOfRepetitions" access="readWrite">
            <description>
            See description in {{param|#.UDPEchoDiagnostics.NumberOfRepetitions}}.
            </description>
            <syntax>
              <unsignedInt>
                 <range minInclusive="1" step="1"/>
              </unsignedInt>
              <default value="3"/>
           </syntax>
        </parameter>
        <parameter name="Timeout" access="readWrite">
            <description>
            See description in {{param|#.UDPEchoDiagnostics.Timeout}}.
            </description>
            <syntax>
              <unsignedInt>
                 <range minInclusive="1" step="1"/>
                 <units value="milliseconds"/>
              </unsignedInt>
              <default value="100"/>
           </syntax>
        </parameter>
        <parameter name="DataBlockSize" access="readWrite">
            <description>
            See description in {{param|#.UDPEchoDiagnostics.DataBlockSize}}.
            </description>
            <syntax>
              <unsignedInt>
                 <range minInclusive="1" maxInclusive="65535" step="1"/>
                 <units value="bytes"/>
              </unsignedInt>
              <default value="1000"/>
           </syntax>
        </parameter>
        <parameter name="DSCP" access="readWrite">
            <description>
            See description in {{param|#.UDPEchoDiagnostics.DSCP}}.
            </description>
            <syntax>
              <unsignedInt>
                 <range minInclusive="0" maxInclusive="63" step="1"/>
              </unsignedInt>
              <default value="0"/>
           </syntax>
        </parameter>
        <parameter name="X_ADB_TrafficClass" access="readWrite">
            <description>
            See description in {{param|#.UDPEchoDiagnostics.X_ADB_TrafficClass}}.
            </description>
            <syntax>
                <unsignedInt>
                    <range minInclusive="0" maxInclusive="0"/>
                    <range minInclusive="240" maxInclusive="255" step="1"/>
                </unsignedInt>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="InterTransmissionTime" access="readWrite">
            <description>
            See description in {{param|#.UDPEchoDiagnostics.InterTransmissionTime}}.
            </description>
            <syntax>
              <unsignedInt>
                 <range minInclusive="1" maxInclusive="65534"/>
                 <units value="milliseconds"/>
              </unsignedInt>
              <default value="100"/>
           </syntax>
        </parameter>
        <parameter name="SuccessCount" access="readOnly" activeNotify="canDeny">
            <description>
            See description in {{param|#.UDPEchoDiagnostics.SuccessCount}}.
            </description>
            <syntax>
              <unsignedInt/>
              <default value="0"/>
           </syntax>
        </parameter>
        <parameter name="FailureCount" access="readOnly" activeNotify="canDeny">
            <description>
            See description in {{param|#.UDPEchoDiagnostics.FailureCount}}.
            </description>
            <syntax>
              <unsignedInt/>
              <default value="0"/>
           </syntax>
        </parameter>
        <parameter name="X_ADB_StartTime" access="readOnly">
            <description>
            After the test is requested, this parameter contains the absolute start time of the UDPEcho test.
            </description>
            <syntax>
                <dateTime/>
                <default value="0001-01-01T00:00:00Z"/>
            </syntax>
        </parameter>
        <parameter name="AverageResponseTime" access="readOnly" activeNotify="canDeny">
            <description>
            See description in {{param|#.UDPEchoDiagnostics.AverageResponseTime}}.
            </description>
            <syntax>
              <unsignedInt>
                 <units value="milliseconds"/>
              </unsignedInt>
              <default value="0"/>
           </syntax>
        </parameter>
        <parameter name="MaximumResponseTime" access="readOnly" activeNotify="canDeny">
            <description>
            See description in {{param|#.UDPEchoDiagnostics.MaximumResponseTime}}.
            </description>
            <syntax>
              <unsignedInt>
                 <units value="milliseconds"/>
              </unsignedInt>
              <default value="0"/>
           </syntax>
        </parameter>
        <parameter name="MinimumResponseTime" access="readOnly" activeNotify="canDeny">
            <description>
            See description in {{param|#.UDPEchoDiagnostics.MinimumResponseTime}}.
            </description>
            <syntax>
              <unsignedInt>
                 <units value="milliseconds"/>
              </unsignedInt>
              <default value="0"/>
           </syntax>
        </parameter>
        <parameter name="X_ADB_IndividualPacketResults" access="readWrite">
            <description>
            Collect {{object|IndividualPacketResult}} objects.
            </description>
            <syntax>
              <boolean/>
              <default value="true"/>
           </syntax>
        </parameter>
        <parameter name="IndividualPacketResultNumberOfEntries" access="readOnly" store="no">
            <description>{{numentries}}</description>
            <syntax>
                <unsignedInt/>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="X_ADB_Reset" access="readOnly" set="ResetTest.sh" aliasOnGet="%s(false)" store="no">
            <description>
                When set {{true}} all optional paramaters are reset
            </description>
            <syntax>
                <boolean/>
                <default value="false"/>
            </syntax>
        </parameter>
    </object>
    <object name="IP.Diagnostics.X_ADB_UDPEchoDiagnostics.{i}.IndividualPacketResult.{i}." access="readWrite" minEntries="0" maxEntries="unbounded" numEntriesParameter="IndividualPacketResultNumberOfEntries">
        <description>
