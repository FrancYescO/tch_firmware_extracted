           </syntax>
        </parameter>
        <parameter name="TestRespSN" access="readOnly" activeNotify="canDeny">
            <description>
            See description in {{param|##.UDPEchoDiagnostics.IndividualPacketResult.{i}.TestRespSN}}.
            </description>
            <syntax>
              <unsignedInt/>
              <default value="0"/>
           </syntax>
        </parameter>
        <parameter name="TestRespRcvTimeStamp" access="readOnly" activeNotify="canDeny">
            <description>
            See description in {{param|##.UDPEchoDiagnostics.IndividualPacketResult.{i}.TestRespRcvTimeStamp}}.
            </description>
            <syntax>
              <unsignedInt>
                  <units value="microseconds"/>
              </unsignedInt>
              <default value="0"/>
           </syntax>
        </parameter>
        <parameter name="TestRespReplyTimeStamp" access="readOnly" activeNotify="canDeny">
            <description>
            See description in {{param|##.UDPEchoDiagnostics.IndividualPacketResult.{i}.TestRespReplyTimeStamp}}.
            </description>
            <syntax>
              <unsignedInt>
                  <units value="microseconds"/>
              </unsignedInt>
              <default value="0"/>
           </syntax>
        </parameter>
        <parameter name="TestRespReplyFailureCount" access="readOnly" activeNotify="canDeny">
            <description>
            See description in {{param|##.UDPEchoDiagnostics.IndividualPacketResult.{i}.TestRespReplyFailureCount}}.
            </description>
            <syntax>
              <unsignedInt/>
              <default value="0"/>
           </syntax>
        </parameter>
    </object>
