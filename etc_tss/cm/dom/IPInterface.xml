<?xml version="1.0" encoding="UTF-8"?>
<datamodel name="IPInterface" version="2.0">
    <object name="IP." access="readOnly" minEntries="1" maxEntries="1">
        <parameter name="InterfaceNumberOfEntries" access="readOnly" store="no">
            <syntax>
                <unsignedInt/>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="IPv4Capable" access="readOnly">
            <syntax>
                <boolean/>
                <default value="true"/>
            </syntax>
        </parameter>
        <parameter name="IPv4Enable" access="readWrite" supported="no">
            <syntax>
                <boolean/>
                <default value="true"/>
            </syntax>
        </parameter>
        <parameter name="IPv4Status" access="readOnly" store="no">
            <syntax>
                <string>
                    <enumeration value="Disabled"/>
                    <enumeration value="Enabled"/>
                    <enumeration value="Error"/>
                </string>
                <default value="Enabled"/>
            </syntax>
        </parameter>
    </object>
    <object name="IP.Interface.{i}." access="readWrite" minEntries="0" maxEntries="unbounded"
            numEntriesParameter="InterfaceNumberOfEntries" enableParameter="Enable" set="IPIf.sh">
        <parameter name="Alias" access="readWrite">
            <syntax>
                <dataType ref="Alias"/>
            </syntax>
        </parameter>
        <parameter name="AutoIPEnable" access="readWrite" supported="no">
            <syntax>
                <boolean/>
                <default value="false"/>
            </syntax>
        </parameter>
        <parameter name="Enable" access="readWrite">
            <syntax>
                <boolean/>
                <default value="false"/>
            </syntax>
        </parameter>
        <parameter name="IPv4AddressNumberOfEntries" access="readOnly" store="no">
            <syntax>
                <unsignedInt/>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="IPv4Enable" access="readWrite" supported="no">
            <syntax>
                <boolean/>
                <default value="true"/>
            </syntax>
        </parameter>
        <parameter name="LastChange" access="readOnly" activeNotify="canDeny" store="no" get="IPIf.sh">
            <syntax>
                <unsignedInt>
                    <units value="seconds"/>
                </unsignedInt>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="Loopback" access="readWrite" supported="no">
            <syntax>
                <boolean/>
                <default value="false"/>
            </syntax>
        </parameter>
        <parameter name="LowerLayers" access="readWrite">
            <syntax>
                <list>
                    <size minLength="0" maxLength="1024"/>
                </list>
                <string>
                    <pathRef targetParent=".Ethernet.Link." refType="strong" targetType="row"/>
                    <pathRef targetParent=".Ethernet.VLANTermination." refType="strong" targetType="row"/>
                    <pathRef targetParent=".PPP.Interface." refType="strong" targetType="row"/>
                    <pathRef targetParent=".ATM.Link." refType="strong" targetType="row"/>
                </string>
                <default value=""/>
            </syntax>
        </parameter>
        <parameter name="MaxMTUSize" access="readWrite">
            <syntax>
                <unsignedInt>
                    <range minInclusive="64" maxInclusive="65535" step="1"/>
                </unsignedInt>
                <default value="1500"/>
            </syntax>
        </parameter>
        <parameter name="Name" access="readOnly">
            <syntax>
                <string>
                    <size minLength="0" maxLength="64"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="Reset" access="readWrite">
            <syntax hidden="true">
                <boolean/>
                <default value="false"/>
            </syntax>
        </parameter>
        <parameter name="Router" access="readWrite">
            <syntax>
                <string>
                    <size minLength="0" maxLength="256"/>
                    <pathRef targetParent=".Routing.Router." refType="strong" targetType="row"/>
                </string>
                <default value="Device.Routing.Router.1"/>
            </syntax>
        </parameter>
        <parameter name="Status" access="readOnly" store="no" localNotify="forceEnabled">
            <syntax>
                <string>
                    <enumeration value="Up"/>
                    <enumeration value="Down"/>
                    <enumeration value="Unknown"/>
                    <enumeration value="Dormant"/>
                    <enumeration value="NotPresent"/>
                    <enumeration value="LowerLayerDown"/>
                    <enumeration value="Error"/>
                </string>
                <default value="LowerLayerDown"/>
            </syntax>
        </parameter>
        <parameter name="Type" access="readOnly" supported="no">
            <syntax>
                <string>
                    <enumeration value="Normal"/>
                    <enumeration value="Loopback"/>
                    <enumeration value="Tunnel"/>
                    <enumeration value="Tunneled"/>
                </string>
                <default value="Normal"/>
            </syntax>
        </parameter>
    </object>
    <object name="IP.Interface.{i}.IPv4Address.{i}." access="readWrite" minEntries="0" maxEntries="unbounded"
            numEntriesParameter="IPv4AddressNumberOfEntries" enableParameter="Enable">
        <parameter name="AddressingType" access="readOnly">
            <syntax>
                <string>
                    <enumeration value="DHCP"/>
                    <enumeration value="AutoIP"/>
                    <enumeration value="IPCP"/>
                    <enumeration value="Static"/>
                </string>
                <default value="Static"/>
            </syntax>
        </parameter>
        <parameter name="Alias" access="readWrite">
            <syntax>
                <dataType ref="Alias"/>
            </syntax>
        </parameter>
        <parameter name="Enable" access="readWrite">
            <syntax>
                <boolean/>
                <default value="false"/>
            </syntax>
        </parameter>
        <parameter name="Status" access="readOnly" store="no">
            <syntax>
                <string>
                    <enumeration value="Disabled"/>
                    <enumeration value="Enabled"/>
                    <enumeration value="Error_Misconfigured"/>
                </string>
                <default value="Disabled"/>
            </syntax>
        </parameter>
        <parameter name="IPAddress" access="readWrite">
            <syntax>
                <dataType ref="IPv4Address"/>
            </syntax>
        </parameter>
        <parameter name="SubnetMask" access="readWrite">
            <syntax>
                <dataType ref="IPv4Address"/>
                <default value=""/>
            </syntax>
        </parameter>
    </object>
    <object name="IP.Interface.{i}.Stats." access="readOnly" minEntries="1" maxEntries="1" store="no">
        <parameter name="BroadcastPacketsReceived" access="readOnly" activeNotify="canDeny" supported="no">
            <syntax>
                <unsignedLong/>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="BroadcastPacketsSent" access="readOnly" activeNotify="canDeny" supported="no">
            <syntax>
                <unsignedLong/>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="BytesReceived" access="readOnly" activeNotify="canDeny" get="IPIf.sh">
            <syntax>
                <unsignedLong/>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="BytesSent" access="readOnly" activeNotify="canDeny" get="IPIf.sh">
            <syntax>
                <unsignedLong/>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="DiscardPacketsReceived" access="readOnly" activeNotify="canDeny" supported="no">
            <syntax>
                <unsignedInt/>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="DiscardPacketsSent" access="readOnly" activeNotify="canDeny" supported="no">
            <syntax>
                <unsignedInt/>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="ErrorsReceived" access="readOnly" activeNotify="canDeny" get="IPIf.sh">
            <syntax>
                <unsignedInt/>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="ErrorsSent" access="readOnly" activeNotify="canDeny" get="IPIf.sh">
            <syntax>
                <unsignedInt/>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="MulticastPacketsReceived" access="readOnly" activeNotify="canDeny" supported="no">
            <syntax>
                <unsignedLong/>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="MulticastPacketsSent" access="readOnly" activeNotify="canDeny" supported="no">
            <syntax>
                <unsignedLong/>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="PacketsReceived" access="readOnly" activeNotify="canDeny" get="IPIf.sh">
            <syntax>
                <unsignedLong/>
            </syntax>
        </parameter>
        <parameter name="PacketsSent" access="readOnly" activeNotify="canDeny" get="IPIf.sh">
            <syntax>
                <unsignedLong/>
            </syntax>
        </parameter>
        <parameter name="UnicastPacketsReceived" access="readOnly" activeNotify="canDeny" supported="no">
            <syntax>
                <unsignedLong/>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="UnicastPacketsSent" access="readOnly" activeNotify="canDeny" supported="no">
            <syntax>
                <unsignedLong/>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="UnknownProtoPacketsReceived" access="readOnly" activeNotify="canDeny" supported="no">
            <syntax>
                <unsignedInt/>
                <default value="0"/>
            </syntax>
        </parameter>
    </object>
</datamodel>
