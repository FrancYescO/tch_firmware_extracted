<?xml version="1.0" encoding="UTF-8"?>
<datamodel name="DeviceInfo" version="2.0">
    <object name="." access="readOnly" minEntries="1" maxEntries="1">
        <parameter name="DeviceSummary" access="readOnly" forcedInform="true" store="no">
            <syntax>
                <string/>
            </syntax>
        </parameter>
    </object>
    <object name="DeviceInfo." access="readOnly" minEntries="1" maxEntries="1">
        <parameter name="AdditionalHardwareVersion" access="readOnly">
            <syntax>
                <list>
                    <size maxLength="64"/>
                </list>
                <string/>
                <default value=""/>
            </syntax>
        </parameter>
        <parameter name="AdditionalSoftwareVersion" access="readOnly">
            <syntax>
                <list>
                    <size maxLength="64"/>
                </list>
                <string/>
                <default value=""/>
            </syntax>
        </parameter>
        <parameter name="Description" access="readOnly" activeNotify="canDeny">
            <syntax>
                <string>
                    <size minLength="0" maxLength="256"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="FirstUseDate" access="readOnly" activeNotify="canDeny">
            <syntax>
                <dateTime/>
                <default value="0001-01-01T00:00:00Z"/>
            </syntax>
        </parameter>
        <parameter name="HardwareVersion" access="readOnly" forcedInform="true">
            <syntax>
                <string>
                    <size minLength="0" maxLength="64"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="Manufacturer" access="readOnly">
            <syntax>
                <string>
                    <size minLength="0" maxLength="64"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="ManufacturerOUI" access="readOnly">
            <syntax>
                <string>
                    <size minLength="6" maxLength="6"/>
                    <pattern value="[0-9A-F]{6}"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="ModelName" access="readOnly" activeNotify="canDeny">
            <syntax>
                <string>
                    <size minLength="0" maxLength="64"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="ProductClass" access="readOnly">
            <syntax>
                <string>
                    <size minLength="0" maxLength="64"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="ProvisioningCode" access="readWrite" activeNotify="forceEnabled" forcedInform="true">
            <syntax>
                <string>
                    <size minLength="0" maxLength="64"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="SerialNumber" access="readOnly">
            <syntax>
                <string>
                    <size minLength="0" maxLength="64"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="SoftwareVersion" access="readOnly" activeNotify="forceEnabled" forcedInform="true">
            <syntax>
                <string>
                    <size minLength="0" maxLength="64"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="UpTime" access="readOnly" activeNotify="canDeny" get="DeviceInfo.sh" store="no">
            <syntax>
                <unsignedInt>
                    <units value="seconds"/>
                </unsignedInt>
            </syntax>
        </parameter>
        <parameter name="VendorConfigFileNumberOfEntries" access="readOnly" store="no">
            <syntax>
                <unsignedInt/>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="VendorLogFileNumberOfEntries" access="readOnly" store="no">
            <syntax>
                <unsignedInt/>
                <default value="0"/>
            </syntax>
        </parameter>
    </object>
    <object name="DeviceInfo.VendorConfigFile.{i}." access="readOnly" minEntries="0" maxEntries="unbounded" numEntriesParameter="VendorConfigFileNumberOfEntries">
        <parameter name="Alias" access="readWrite">
            <syntax>
                <dataType ref="Alias"/>
            </syntax>
        </parameter>
        <parameter name="Name" access="readOnly">
            <syntax>
                <string>
                    <size minLength="0" maxLength="64"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="Version" access="readOnly">
            <syntax>
                <string>
                    <size minLength="0" maxLength="16"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="Date" access="readOnly">
            <syntax>
                <dateTime/>
            </syntax>
        </parameter>
        <parameter name="Description" access="readOnly">
            <syntax>
                <string>
                    <size minLength="0" maxLength="256"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="UseForBackupRestore" access="readOnly">
            <syntax>
                <boolean/>
                <default value="true"/>
            </syntax>
        </parameter>
    </object>
    <object name="DeviceInfo.VendorLogFile.{i}." access="readOnly" minEntries="0" maxEntries="unbounded" numEntriesParameter="VendorLogFileNumberOfEntries">
        <parameter name="Alias" access="readWrite">
            <syntax>
                <dataType ref="Alias"/>
            </syntax>
        </parameter>
        <parameter name="Name" access="readOnly">
            <syntax>
                <string>
                    <size maxLength="64"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="MaximumSize" access="readOnly">
            <syntax>
                <unsignedInt>
                    <units value="bytes"/>
                </unsignedInt>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="Persistent" access="readOnly">
            <syntax>
                <boolean/>
                <default value="false"/>
            </syntax>
        </parameter>
    </object>
    <object name="DeviceInfo.MemoryStatus." access="readOnly" minEntries="1" maxEntries="1" store="no">
        <parameter name="Free" access="readOnly" activeNotify="canDeny" get="MemoryStatus.sh">
            <syntax>
                <unsignedInt>
                    <units value="kilobytes"/>
                </unsignedInt>
            </syntax>
        </parameter>
        <parameter name="Total" access="readOnly" get="MemoryStatus.sh">
            <syntax>
                <unsignedInt>
                    <units value="kilobytes"/>
                </unsignedInt>
            </syntax>
        </parameter>
    </object>
    <object name="DeviceInfo.ProcessStatus." access="readOnly" minEntries="1" maxEntries="1" store="no">
        <parameter name="CPUUsage" access="readOnly" activeNotify="canDeny" get="ProcessStatus.sh">
            <syntax>
                <unsignedInt>
                    <range maxInclusive="100" step="1"/>
                    <units value="percent"/>
                </unsignedInt>
            </syntax>
        </parameter>
        <parameter name="ProcessNumberOfEntries" access="readOnly">
            <syntax>
                <unsignedInt/>
                <default value="0"/>
            </syntax>
        </parameter>
    </object>
    <object name="DeviceInfo.ProcessStatus.Process.{i}." access="readOnly" minEntries="0" maxEntries="unbounded"
            numEntriesParameter="ProcessNumberOfEntries" store="no" supported="no">
        <parameter name="CPUTime" access="readOnly" activeNotify="canDeny" supported="no">
            <syntax>
                <unsignedInt>
                    <units value="milliseconds"/>
                </unsignedInt>
            </syntax>
        </parameter>
        <parameter name="Command" access="readOnly" supported="no">
            <syntax>
                <string>
                    <size minLength="0" maxLength="256"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="PID" access="readOnly" key="yes" supported="no">
            <syntax>
                <unsignedInt/>
            </syntax>
        </parameter>
        <parameter name="Priority" access="readOnly" supported="no">
            <syntax>
                <unsignedInt>
                    <range maxInclusive="99" step="1"/>
                </unsignedInt>
            </syntax>
        </parameter>
        <parameter name="Size" access="readOnly" activeNotify="canDeny" supported="no">
            <syntax>
                <unsignedInt>
                    <units value="kilobytes"/>
                </unsignedInt>
            </syntax>
        </parameter>
        <parameter name="State" access="readOnly" activeNotify="canDeny" supported="no">
            <syntax>
                <string>
                    <enumeration value="Running"/>
                    <enumeration value="Sleeping"/>
                    <enumeration value="Stopped"/>
                    <enumeration value="Idle"/>
                    <enumeration value="Uninterruptible"/>
                    <enumeration value="Zombie"/>
                </string>
            </syntax>
        </parameter>
    </object>
</datamodel>
