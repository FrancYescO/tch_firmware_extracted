<?xml version="1.0" encoding="UTF-8"?>
<datamodel name="ManagementServer" version="2.0">
    <object name="ManagementServer." access="readOnly" minEntries="1" maxEntries="1">
        <parameter name="CWMPRetryIntervalMultiplier" access="readWrite">
            <syntax>
                <unsignedInt>
                    <range minInclusive="1000" maxInclusive="65535" step="1"/>
                </unsignedInt>
                <default value="2000"/>
            </syntax>
        </parameter>
        <parameter name="CWMPRetryMinimumWaitInterval" access="readWrite">
            <syntax>
                <unsignedInt>
                    <range minInclusive="1" maxInclusive="65535" step="1"/>
                    <units value="seconds"/>
                </unsignedInt>
                <default value="5"/>
            </syntax>
        </parameter>
        <parameter name="ConnectionRequestPassword" access="readWrite" localNotify="forceEnabled">
            <syntax mangled="true" hidden="true">
                <string>
                    <size minLength="0" maxLength="256"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="ConnectionRequestURL" access="readOnly" activeNotify="forceDefaultEnabled" forcedInform="true">
            <syntax>
                <string>
                    <size minLength="0" maxLength="256"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="ConnectionRequestUsername" access="readWrite" localNotify="forceEnabled">
            <syntax>
                <string>
                    <size minLength="0" maxLength="256"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="DefaultActiveNotificationThrottle" access="readWrite" localNotify="forceEnabled">
            <syntax>
                <unsignedInt>
                    <units value="seconds"/>
                </unsignedInt>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="DownloadProgressURL" access="readOnly">
            <syntax>
                <string>
                    <size minLength="0" maxLength="256"/>
                </string>
                <default value=""/>
            </syntax>
        </parameter>
        <parameter name="KickURL" access="readOnly">
            <syntax>
                <string>
                    <size minLength="0" maxLength="256"/>
                </string>
                <default value=""/>
            </syntax>
        </parameter>
        <parameter name="EnableCWMP" access="readWrite">
            <syntax>
                <boolean/>
                <default value="true"/>
            </syntax>
        </parameter>
        <parameter name="ManageableDeviceNumberOfEntries" access="readOnly" store="no">
            <syntax>
                <unsignedInt/>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="ManageableDeviceNotificationLimit" access="readWrite" localNotify="forceEnabled">
            <syntax>
                <unsignedInt>
                    <units value="seconds"/>
                </unsignedInt>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="ParameterKey" access="readOnly" activeNotify="canDeny" forcedInform="true">
            <syntax>
                <string>
                    <size minLength="0" maxLength="32"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="Password" access="readWrite" localNotify="forceEnabled">
            <syntax mangled="true" hidden="true">
                <string>
                    <size minLength="0" maxLength="256"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="PeriodicInformEnable" access="readWrite" localNotify="forceEnabled">
            <syntax>
                <boolean/>
                <default value="false"/>
            </syntax>
        </parameter>
        <parameter name="PeriodicInformInterval" access="readWrite" localNotify="forceEnabled">
            <syntax>
                <unsignedInt>
                    <range minInclusive="1" step="1"/>
                    <units value="seconds"/>
                </unsignedInt>
                <default value="9999"/>
            </syntax>
        </parameter>
        <parameter name="PeriodicInformTime" access="readWrite" localNotify="forceEnabled">
            <syntax>
                <dateTime/>
                <default value="0001-01-01T00:00:00Z"/>
            </syntax>
        </parameter>
        <parameter name="URL" access="readWrite" localNotify="forceEnabled">
            <syntax>
                <string>
                    <size minLength="0" maxLength="256"/>
                    <pattern value=""/>
                    <pattern value="https?://[-_a-zA-Z0-9\\.]+(:[0-9]{1,5})?(/.*)?"/>
                    <pattern value="https?://\[[a-fA-F0-9:\\.]+\](:[0-9]{1,5})?(/.*)?"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="UpgradesManaged" access="readWrite">
            <syntax>
                <boolean/>
                <default value="false"/>
            </syntax>
        </parameter>
        <parameter name="Username" access="readWrite" localNotify="forceEnabled">
            <syntax>
                <string>
                    <size minLength="0" maxLength="256"/>
                </string>
            </syntax>
        </parameter>
    </object>
    <object name="ManagementServer.ManageableDevice.{i}." access="readOnly" minEntries="0" maxEntries="unbounded" numEntriesParameter="ManageableDeviceNumberOfEntries">
        <parameter name="Alias" access="readWrite" key="yes">
            <syntax>
                <dataType ref="Alias"/>
            </syntax>
        </parameter>
        <parameter name="Host" access="readOnly">
            <syntax>
                <list>
                    <size minLength="0" maxLength="1024"/>
                </list>
                <string>
                    <pathRef targetParent=".Hosts.Host." refType="strong" targetType="row"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="ManufacturerOUI" access="readOnly" activeNotify="canDeny">
            <syntax>
                <string>
                    <size minLength="6" maxLength="6"/>
                    <pattern value="[0-9A-F]{6}"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="ProductClass" access="readOnly" activeNotify="canDeny">
            <syntax>
                <string>
                    <size minLength="0" maxLength="64"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="SerialNumber" access="readOnly" activeNotify="canDeny">
            <syntax>
                <string>
                    <size minLength="0" maxLength="64"/>
                </string>
            </syntax>
        </parameter>
    </object>
</datamodel>
