<?xml version="1.0" encoding="UTF-8"?>
<datamodel name="QoEDiagnostics" version="2.0">
    <object name="IP.Diagnostics.">
        <parameter name="IP.Diagnostics.X_ADB_YoutubeDiagnosticsNumberOfEntries" access="readOnly" store="no">
            <description>{{numentries}}</description>
            <syntax>
                <unsignedInt/>
                <default value="0"/>
            </syntax>
        </parameter>
    </object>
    <object name="IP.Diagnostics.X_ADB_YoutubeDiagnostics." minEntries="1" maxEntries="unbounded" set="Youtube.sh">
        <parameter name="DiagnosticsState" access="readWrite">
            <description>{{enum}}
            See description in {{param|#.DownloadDiagnostics.DiagnosticsState}}.
            </description>
            <syntax>
                <string>
                    <enumeration value="None"/>
                    <enumeration value="Requested"/>
                    <enumeration value="Completed"/>
                    <enumeration value="Error_TestFail"/>
                    <enumeration value="Error_NoURL"/>
                </string>
                <default value="None"/>
            </syntax>
        </parameter>
        <parameter name="Alias" access="readOnly" key="yes">
            <description>Name of this object instance.</description>
            <syntax>
                <dataType ref="Alias"/>
            </syntax>
        </parameter>
        <parameter name="URL" access="readWrite">
            <description>
                This value will be configured in the {{param|.IP.Diagnostics.X_ADB_YoutubeDiagnostics.{i}.DownloadURL}} parameter of the instances that will be created during this test campaign.
            </description>
            <syntax>
                <string>
                    <size minLength="0" maxLength="256"/>
                    <pattern value="http://.+"/>
                    <pattern value="https://.+"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="ProtocolVersion" access="readWrite">
            <description>
                This value if other than any, force ip protocol version during test.
            </description>
            <syntax>
                <string>
                    <enumeration value="Any"/>
                    <enumeration value="IPv4"/>
                    <enumeration value="IPv6"/>
                </string>
                <default value="Any"/>
            </syntax>
        </parameter>
        <parameter name="BufferSize" access="readWrite">
            <description>
                Length of playout buffer [sec]
            </description>
            <syntax>
                <unsignedInt/>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="MinTime" access="readWrite">
            <description>
                Test is considered failed if it terminates before this time [sec]
            </description>
            <syntax>
                <unsignedInt/>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="MaxTime" access="readWrite">
            <description>
                Finish test if takes more then this time [sec]
            </description>
            <syntax>
                <unsignedInt/>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="BitRate" access="readWrite">
            <description>
                If greater than 0, the Test Agent will attempt to download test stream at this fixed bitrate [bps]
            </description>
            <syntax>
                <unsignedInt/>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="OneBitRate" access="readWrite">
            <description>
                When set to true, the client does not switch to lower bit rate when a stall occurs
            </description>
            <syntax>
              <boolean/>
              <default value="false"/>
           </syntax>
        </parameter>
        <parameter name="Stalls" access="readOnly" activeNotify="canDeny">
            <description>
                Number of detected stalls
            </description>
            <syntax>
                <unsignedInt/>
            </syntax>
        </parameter>
        <parameter name="AverageStallDuration" access="readOnly" activeNotify="canDeny">
            <description>
                Average Stall duration during video download [usec]
            </description>
            <syntax>
                <unsignedLong/>
            </syntax>
        </parameter>
        <parameter name="TotalStallDuration" access="readOnly" activeNotify="canDeny">
            <description>
                Total Stall duration during video download [usec]
            </description>
            <syntax>
                <unsignedLong/>
            </syntax>
        </parameter>
        <parameter name="MaxBitRate" access="readOnly" activeNotify="canDeny">
            <description>
                Max bitrate during test [bps]
            </description>
            <syntax>
                <unsignedInt/>
            </syntax>
        </parameter>
        <parameter name="MinBitRate" access="readOnly" activeNotify="canDeny">
            <description>
                Min bitrate during test [bps]
            </description>
            <syntax>
                <unsignedInt/>
            </syntax>
        </parameter>
        <parameter name="AdvertisedBitRate" access="readOnly" activeNotify="canDeny">
            <description>
                Stream encoding bitrate (audio + video) [bps]
            </description>
            <syntax>
                <unsignedInt/>
            </syntax>
        </parameter>
        <parameter name="PlaybackTime" access="readOnly" activeNotify="canDeny">
            <description>
                Total time of video test [usec]
            </description>
            <syntax>
                <unsignedLong/>
            </syntax>
        </parameter>
        <parameter name="ITag" access="readOnly" activeNotify="canDeny">
            <description>
                Youtube video itag
            </description>
            <syntax>
                <unsignedInt/>
            </syntax>
        </parameter>
        <parameter name="X_ADB_Reset" access="readOnly" set="ResetTest.sh" aliasOnGet="%s(false)" store="no">
            <description>
                When set {{true}} all optional paramaters are reset
            </description>
            <syntax>
                <boolean/>
                <default value="false"/>
            </syntax>
        </parameter>
    </object>
</datamodel>
