    <object name="IP.Diagnostics.DownloadDiagnostics." access="readOnly" minEntries="1" maxEntries="1" store="no" set="DownloadUploadDiagnostics.sh">
        <parameter name="DiagnosticsState" access="readWrite" activeNotify="canDeny" localNotify="forceEnabled">
            <syntax>
                <string>
                    <enumeration value="None"/>
                    <enumeration value="Requested"/>
                    <enumeration value="Completed"/>
                    <enumeration value="Error_InitConnectionFailed"/>
                    <enumeration value="Error_NoResponse"/>
                    <enumeration value="Error_TransferFailed"/>
                    <enumeration value="Error_PasswordRequestFailed"/>
                    <enumeration value="Error_LoginFailed"/>
                    <enumeration value="Error_NoTransferMode"/>
                    <enumeration value="Error_NoPASV"/>
                    <enumeration value="Error_IncorrectSize"/>
                    <enumeration value="Error_Timeout"/>
                    <enumeration value="Error_NoCWD"/>
                </string>
                <default value="None"/>
            </syntax>
        </parameter>
        <parameter name="Interface" access="readWrite">
            <syntax>
                <string>
                    <size minLength="0" maxLength="256"/>
                    <pathRef targetParent=".IP.Interface." refType="strong" targetType="row"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="DownloadURL" access="readWrite">
            <syntax>
                <string>
                    <size minLength="0" maxLength="256"/>
                </string>
            </syntax>
        </parameter>
        <parameter name="DownloadTransports" access="readOnly">
            <syntax>
                <list maxItems="2"/>
                <string>
                    <enumeration value="HTTP"/>
                    <enumeration value="FTP"/>
                </string>
                <default value="HTTP,FTP"/>
            </syntax>
        </parameter>
        <parameter name="DSCP" access="readWrite">
            <syntax>
                <unsignedInt>
                    <range minInclusive="0" maxInclusive="63" step="1"/>
                </unsignedInt>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="EthernetPriority" access="readWrite">
            <syntax>
                <unsignedInt>
                    <range minInclusive="0" maxInclusive="7" step="1"/>
                </unsignedInt>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="ROMTime" access="readOnly" activeNotify="canDeny">
            <syntax>
                <dateTime/>
                <default value="0001-01-01T00:00:00.000000"/>
            </syntax>
        </parameter>
        <parameter name="BOMTime" access="readOnly" activeNotify="canDeny">
            <syntax>
                <dateTime/>
                <default value="0001-01-01T00:00:00.000000"/>
            </syntax>
        </parameter>
        <parameter name="EOMTime" access="readOnly" activeNotify="canDeny">
            <syntax>
                <dateTime/>
                <default value="0001-01-01T00:00:00.000000"/>
            </syntax>
        </parameter>
        <parameter name="TestBytesReceived" access="readOnly" activeNotify="canDeny">
            <syntax>
                <unsignedInt/>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="TotalBytesReceived" access="readOnly" activeNotify="canDeny">
            <syntax>
                <unsignedInt/>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="TCPOpenRequestTime" access="readOnly" activeNotify="canDeny">
            <syntax>
                <dateTime/>
                <default value="0001-01-01T00:00:00.000000"/>
            </syntax>
        </parameter>
        <parameter name="TCPOpenResponseTime" access="readOnly" activeNotify="canDeny">
            <syntax>
                <dateTime/>
                <default value="0001-01-01T00:00:00.000000"/>
            </syntax>
        </parameter>
        <parameter name="TimeBasedTestDuration" access="readWrite">
            <description action="replace">
            When {{param}} > 0 the download is time based rather than a file size based test.
            This is a 3 digit variable and its value is between 1-999 seconds.
            The file size must be larger than the time it takes to transfer the file.
            </description>
            <syntax>
                <unsignedInt>
                    <range minInclusive="0" maxInclusive="999" step="1"/>
                    <units value="seconds"/>
                </unsignedInt>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="TimeBasedTestIncrements" access="readWrite">
            <description action="replace">
            When {{param}} > 0, FTP/HTTP downloads are time segmented into subintervals (i.e. time slots), each having a duration of {{param}} (seconds).
            This is to allow time based transfer rather than file size based transfer.
            Defaults to zero, which implies time segmenting is disabled.
            </description>
            <syntax>
                <unsignedInt>
                    <units value="seconds"/>
                </unsignedInt>
                <default value="0"/>
            </syntax>
        </parameter>
        <parameter name="TimeBasedTestIncrementsOffset" access="readWrite">
            <description action="replace">
            {{param|TimeBasedTestIncrements}} segments HTTP/FTP downloads into subintervals of duration {{param|TimeBasedTestIncrements}} starting from {{param|BOMTime}}.
            However, when {{param}} > 0, the {{param|TimeBasedTestIncrements}} starts at time {{param|BOMTime}} + {{param}}.
            It is used to remove the contribution from slow start in a throughput measurement.
            </description>
            <syntax>
                <unsignedInt>
                    <units value="seconds"/>
                </unsignedInt>
                <default value="0"/>
            </syntax>
        </parameter>
    </object>
    <object name="IP.Diagnostics.DownloadDiagnostics.IncrementalResults.{i}." access="readWrite" minEntries="0" maxEntries="unbounded">
        <description action="replace">
        Results for time segmented throughput tests.
        A new object is created every {{param|#.TimeBasedTestIncrements}}.
        </description>
        <parameter name="IncTestBytesReceived" access="readOnly" activeNotify="canDeny">
            <description action="replace">
            Incremental change of bytes received every {{param|#.TimeBasedTestIncrements}} seconds between the last {{param|#.BOMTime}}.
            This value is attributed specifically to the {ith} time interval where each interval is of duration {{param|#.TimeBasedTestIncrements}}.
            </description>
            <syntax>
                <unsignedInt/>
            </syntax>
        </parameter>
        <parameter name="IncBOMTime" access="readOnly" activeNotify="canDeny">
            <description action="replace">
            Empirical measurement of {{param|#.BOMTime}} every {{param|#.TimeBasedTestIncrements}} seconds.
            This value is attributed specifically to the {ith} time interval where each interval is of duration {{param|#.TimeBasedTestIncrements}}.
            </description>
            <syntax>
                <dateTime/>
            </syntax>
        </parameter>
        <parameter name="IncEOMTime" access="readOnly" activeNotify="canDeny">
            <description action="replace">
            Empirical measurement of {{param|#.EOMTime}} every {{param|#.TimeBasedTestIncrements}} seconds.
            This value is attributed specifically to the {ith} time interval where each interval is of duration {{param|#.TimeBasedTestIncrements}}.
            </description>
            <syntax>
                <dateTime/>
            </syntax>
        </parameter>
    </object>
