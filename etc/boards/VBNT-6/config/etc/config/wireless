config wifi-bandsteer 'bs0'
	option policy_mode '5'
	option monitor_window '5'
	option sta_comeback_to '5'
	option debug_flags '0'
	option rssi_5g_threshold '-80'
	option rssi_threshold '-60'
	option no_powersave_steer '0'

