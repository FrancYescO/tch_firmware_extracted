config device_defaults 'device_defaults'
	option enabled '1'
	option radio_pref 'lte? umts'
	option roaming 'international'

config platform 'platform'
    option power_on '1'

config radio_preference
        option name 'auto'
        list radio 'lte?'
        list radio 'umts'

config radio_preference
        option name '4GPreferred'
        list radio 'lte'
        list radio 'umts'

config radio_preference
        option name '4GOnly'
        list radio 'lte'

config radio_preference
        option name '3GPreferred'
        list radio 'umts'
        list radio 'lte?'

config radio_preference
        option name '3GOnly'
        list radio 'umts'

config profile
    option id '1'
    option visible '1'
    option editable '1'
    option name 'internet'
    option pdptype 'ipv4v6'

config profile
    option id '2'
    option visible '1'
    option editable '1'
    option name 'voice'
    option apn 'voice'
    option pdptype 'ipv4v6'

config detector
    option type 'usb'

config mobiled 'globals'
    option tracelevel '4'
    option max_devices '1'
    option initmode 'PlatformConfigure'

