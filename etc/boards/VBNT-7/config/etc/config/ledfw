config service 'internet'
	option check '1'

config service 'iptv'
	option check '0'
	option itf 'wan'

config service 'voip'
	option check '1'

config wifi 'wifi'
	option nsc_on '1'

config timeout 'timeout'
	option ms '60000'

config status_led 'status_led'
	option enable '0'
	option mobile_itf '0'

config syslog 'syslog'
	option trace '0'
