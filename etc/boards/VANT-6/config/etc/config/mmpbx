        list network 'DEFAULT_NETWORK'
        list network 'internal_net'
        list network 'sip_net'
        list event_description 'DELIVERED_RINGBACK'
        list event_description 'DELIVERED_RINGBACK_QUEUED'
        option delay '500'
        option repeat_after '-1'
        list   play 'tripple_freq_tone-1'
        list   duration '400'
        list   play 'silence-1'
        list   duration '200'
        list   play 'tripple_freq_tone-2'
        list   duration '400'
        list   play 'silence-2'
        list   duration '2000'
        list   loop_from 'silence-2'
        list   loop_to 'tripple_freq_tone-1'
        list   loop_iterations '-1'

