        list event_description 'DISCONNECTED_LOCAL_SCCREJECTION'
        list event_description 'DIALING_PROFILE_USABLE_SPECIAL'
        option delay '0'
        option repeat_after '-1'
        list   play 'single_freq_425_13'
        list   duration '-1'

