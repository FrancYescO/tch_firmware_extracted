config tone 'tripple_freq_tone'
        list   frequency '425'
        list   power '-13'
        list   frequency '400'
        list   power '-19'
        list   frequency '450'
        list   power '-19'

config tone 'single_freq_425_13'
        list   frequency '425'
        list   power '-13'

config tone 'single_freq_425_10'
        list   frequency '425'
        list   power '-10'

config tone 'single_freq_425_18'
        list   frequency '425'
        list   power '-18'

config tone 'single_freq_425_20'
        list   frequency '425'
        list   power '-20'

config tone 'silence'
        list   frequency '0'
        list   power '0'

config file 'file_confirmation'
        option filename '/etc/mmpbx/confirmation.au'
        option encoding 'PCMU'

config audionotification 'dial'
        list network 'DEFAULT_NETWORK'
        list network 'internal_net'
        list network 'sip_net'
        list event_description 'DIALING_PROFILE_USABLE'
        option delay '0'
        option repeat_after '-1'
        list   play 'tripple_freq_tone'
        list   duration '-1'

config audionotification 'stutterdial'
        list network 'DEFAULT_NETWORK'
        list network 'sip_net'
        list event_description 'DIALING_PROFILE_USABLE_MESSAGESWAITING'
        list event_description 'DIALING_PROFILE_USABLE_MESSAGESWAITING_SPECIAL'
        option delay '0'
        option repeat_after '-1'
        list   play 'tripple_freq_tone'
        list   duration '100'
        list   play 'silence'
        list   duration '40'
        list   loop_from 'silence'
        list   loop_to 'tripple_freq_tone'
        list   loop_iterations '-1'

config audionotification 'facility'
        list network 'DEFAULT_NETWORK'
        list network 'internal_net'
        list network 'sip_net'
        list event_description 'DIALING_PROFILE_USABLE_CALLFORWARDING'
        list event_description 'DIALING_PROFILE_USABLE_NONSTANDARD'
