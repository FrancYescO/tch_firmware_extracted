uci -q del_list web.ruleset_main.rules=mmpbxinfomodal
uci add_list web.ruleset_main.rules=mmpbxinfomodal

uci -q delete web.mmpbxinfomodal
uci set web.mmpbxinfomodal=rule
uci set web.mmpbxinfomodal.target=/modals/mmpbx-info-modal.lp
uci add_list web.mmpbxinfomodal.roles=admin

