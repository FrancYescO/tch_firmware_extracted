config always 'wifidoctor_enabled'
	option path 'wifi_doctor_agent.config.enabled'
	option value '0'

config always 'wifidoctor_url'
	option path 'wifi_doctor_agent.config.cs_url'
	option value ''

config always 'vsparc_enabled'
	option path 'tls-vsparc.Config.Start'
	option value '0'

config always 'thor_enabled'
	option dir '/etc/config/tls-thor'
	option path 'tls-thor.ProcessManager.Enabled'
	option value '0'
