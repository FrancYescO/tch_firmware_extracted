config assistance 'remote'
    option sessionmgr 'assistance'
    option user       'assist'

config sessionmgr "default"
    option cookiepath   "/"
    option authpath     "/authenticate"
    option passpath     "/password"
    option loginpath    "/login.lp"
    option timeout      "10"
    list   users        "usr_admin"
    list   users        "usr_forgotpassword"
    option ruleset      "ruleset_main"
    option bruteforce '1'
    option relaxfirstattempt '1'
    option maxwrongattempt '15'

config sessionmgr 'assistance'
    option cookiepath '/'
    option authpath '/authenticate'
    option passpath '/password'
    option loginpath	'/login.lp'
    option timeout '30'
    list users 'usr_assist'
    option ruleset 'ruleset_main'
    option maxsessions  '-1'
    option maxsessions_per_ip  '-1'

config sessionmgr 'wandefault'
    option cookiepath '/'
    option authpath '/authenticate'
    option passpath '/password'
    option loginpath '/login.lp'
    option timeout '10'
    list users 'usr_telmexadmin'
    option ruleset 'ruleset_main'
    option maxsessions  '50'
    option maxsessions_per_ip  '10'
    option bruteforce '1'

config uiconfig 'uidefault'
    option defaultuser 'TELMEX'
    option nsplink 'http://www.telmex.com/'
    option upgradefw '1'
    option dnsserver_query '1'

config user "usr_admin"
    option name          "TELMEX"
    option role          "admin"
	option password_reminder '1'

config user "usr_telmexadmin"
    option name          "telmex"
    option role          "admin"

config user "usr_forgotpassword"
    option name "forgotpassword"
    option role "recovery"
    option gak_id "1"
    option srp_salt ""
    option srp_verifier ""

config user 'usr_assist'
    option name 'assist'
    option role 'admin'
    option srp_salt ''
    option srp_verifier ''

config ruleset "ruleset_main"
    list rules        "gateway"
    list rules        "login"
    list rules        "password"
    list rules        "homepage"
    list rules        "sshaccess"
    list rules        "gatewaymodal"
    list rules        "broadbandmodal"
    list rules        "broadbandethmodal"
    list rules        "internetmodal"
    list rules        "internetmodaltpv"
    list rules        "wirelessmodal"
    list rules        "wirelessqrcodemodal"
    list rules        "ethernetmodal"
    list rules        "devicemodal"
    list rules        "ipv6devicemodal"
    list rules        "wanservices"
    list rules        "firewallmodal"
    list rules        "diagnosticsconnectionmodal"
    list rules        "diagnosticsnetworkmodal"
    list rules        "diagnosticspingmodal"
    list rules        "diagnosticsxdslmodal"
    list rules        "diagnosticstcpdumpmodal"
    list rules        "assistancemodal"
    list rules        "usermgrmodal"
    list rules        "syslogmodal"
    list rules        "todmodal"
    list rules        "todwifimodal"
    list rules        "iproutesmodal"
    list rules        "cwmpconf"
    list rules        "xdsllowmodal"
    list rules        "natalghelper"
    list rules        "hostmapmodal"
    list rules        "systemmodal"
    list rules        "wirelessclientmodal"
    list rules        "portmirrormodal"
    list rules        "resetpassword"
    list rules        "speedservice"
    list rules        "wirelessajax"
	 list rules 		 "connectionsmodal"

config rule 'gateway'
    option target     '/gateway.lp'
    list roles        'guest'
    list roles        'admin'
    list roles        'engineer'
    list roles        'recovery'

config rule "login"
    option target       "/login.lp"
    list roles          "guest"
    list roles          "admin"
    list roles          'engineer'
    list roles          'recovery'

config rule "password"
    option target       "/password.lp"
    list roles          "admin"
    list roles          'engineer'

config rule "homepage"
    option target       "/"
    list roles          "guest"
    list roles          "admin"

config rule "sshaccess"
    option target		"/ssh-access.lp"
    list roles			"admin"

