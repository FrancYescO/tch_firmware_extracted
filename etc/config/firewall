config defaults
	option syn_flood	 '1'
	option input		 'ACCEPT'
	option output		 'ACCEPT'
	option forward		 'DROP'
	option drop_invalid	'1'

config zone 'lan'
	option name		 'lan'
	list   network		 'lan'
	option input		 'ACCEPT'
	option output		 'ACCEPT'
	option forward		 'ACCEPT'
    option mtu_fix		 '1'
	option wan		 '0'

