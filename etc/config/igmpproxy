config globals 'globals'
	option trace '0'
	option state '1'
	option requirera '1'
	option qi '125'
	option qri '10'
	option lmqi '1'
	option rv '2'
	option localgroup '0'

config interface 'lan'
	option state 'downstream'
	option fastleave '1'
	option version 'igmpv2'
	option exptrack '1'
  
config interface 'bt_iptv'
	option state 'upstream'
	
config interface 'wan'
	option state 'inactive'

#
# Exclude upnp
#
config noproxy
        option interface 'lan'
        option mcastrange '239.255.255.250'

#
# Exclude qeo
#
config noproxy
        option interface 'lan'
        option mcastrange '239.255.0.1'
