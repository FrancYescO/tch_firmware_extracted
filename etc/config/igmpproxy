config globals 'globals'
	option trace '1'
	option state '1'
	option requirera '1'
	option qi '125'
	option qri '10'
	option lmqi '1'
	option rv '2'
	option advinter '20'
	option initadvinter '2'
	option initadvcount '3'
	option localgroup '0'
config interface 'lan'
	option state 'downstream'
	option version 'igmpv3'
	option fastleave '1'
	option exptrack '1'
	option mrd '0'

config interface 'VLAN1'
	option state 'downstream'
	option version 'igmpv3'
	option fastleave '1'
	option exptrack '1'
	option mrd '0'

config interface 'VLAN2'
	option state 'downstream'
	option version 'igmpv3'
	option fastleave '1'
	option exptrack '1'
	option mrd '0'

config interface 'VLAN3'
	option state 'downstream'
	option version 'igmpv3'
	option fastleave '1'
	option exptrack '1'
	option mrd '0'

config interface 'wan'
        option state 'upstream'

#
# Exclude upnp
#
config noproxy
        option interface 'lan'
        option mcastrange '239.255.255.250'

