config single_config 'cwmpd_config'
  option state '0'
  option upgradesmanaged '1'
  option interface 'wan'
  option periodicinform_enable '1'
  option periodicinform_interval '43200'
  option periodicinform_time '0'
  option connectionrequest_port '51007'
  option ssl_castore '/etc/ssl/certs/'
  option ssl_certificate '/etc/ssl/tch_cwmp_ssl.cert'
  option ssl_privatekey '/etc/ssl/tch_cwmp_ssl.key'
  option ssl_hostnamecheck '1'
  option ssl_verifypeer '1'
  option acs_url 'http://nld-acs.com/'
  option enforce_https '0'
  option connectionrequest_auth '0'
  option upgrade_rollback_timeout '180'
  list forcedactives 'InternetGatewayDevice.Layer3Forwarding.DefaultConnectionService'

