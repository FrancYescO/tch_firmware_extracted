	option state 1
        option upgradesmanaged '1'
	option connectionrequest_auth '0'
	option connectionrequest_port '51050'
	option periodicinform_enable '1'
	option periodicinform_interval '1209600'
	option interface 'wan'
	option ssl_verifypeer '1'
	option enfore_https 'yes'
	list forcedactives 'InternetGatewayDevice.Layer3Forwarding.DefaultConnectionService'
	list notifications 'InternetGatewayDevice.X_FASTWEB_TeleManagement.LastModifiedByUser|Active'
