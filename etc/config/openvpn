config openvpn custom_config
	option enabled 0
	option config /etc/openvpn/my-vpn.conf

config openvpn server
	option enabled 0
	option port 1194
	option proto udp
	option dev tun
	option ca /etc/openvpn/ca.crt
	option cert /etc/openvpn/server.crt
	option key /etc/openvpn/server.key
	option dh /etc/openvpn/dh2048.pem
	option server "10.8.0.0 255.255.255.0"
	option server_bk "10.8.0.0 255.255.255.0"
	option ifconfig_pool_persist /tmp/ipp.txt
	option duplicate_cn 1
	option keepalive "10 120"
#	option tls_auth "/etc/openvpn/ta.key 0"
	option cipher AES-128-CBC
#	option comp_lzo 0
	option persist_key 1
	option persist_tun 1
	option user root
	option status /tmp/openvpn-status.log
	option verb 3
	option script_security 3
	option auth_user_pass_verify "/etc/openvpn/checkpwd.sh via-env"
        option up /etc/openvpn/server_up.sh
        option down /etc/openvpn/server_down.sh

config openvpn client1
	option enabled 0
	option client 1
	option dev tun
	option proto udp
	list remote "my_server 1194"
	option resolv_retry infinite
	option nobind 1
	option persist_key 1
	option persist_tun 1
	option user root
	option ca /etc/openvpn/client1_ca.crt
	option cert /etc/openvpn/client1.crt
	option key /etc/openvpn/client1.key
#	option tls_auth "/etc/openvpn/ta.key 1"
	option cipher x
	option comp_lzo 0
	option verb 3
        option name "client1"
        option script_security 3
        option up /etc/openvpn/client_up.sh
        option down /etc/openvpn/client_down.sh

