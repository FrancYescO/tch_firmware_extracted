config interface 'lan'
        option igmp_snooping '1'
	option mcast_flooding '1'
        option mld_snooping '0'

config nosnooping
        option interface 'lan'
        option mcastrange '239.255.255.250'

config nosnooping
        option interface 'lan'
        option mcastrange '239.255.0.1'

config nosnooping
        option interface 'lan'
        option mcastrange '224.0.0.0-224.0.0.255'
