config global 'global'
    option enabled '0'
    option license '/etc/tch_spdsvc_license.txt'

config server 'server'
    option enabled '0'
    option tcp_port '9090'

config measurement 'default_measurement'
    option mode 'bw'
    option server '192.168.1.2'
    option port '8081'
    option duration '1000'
    option rate '1200000'
    option direction 'us'
    option packetlength '1472'
    option loss '0'
    option latencytolerance '0'
    option algo 'rxrate'
    option maxsteps '1'
    option state 'None'
    option resultcount '3'
