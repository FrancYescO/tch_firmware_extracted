config optical_transceiver 'MSTC_BOSA'
	option vendor_name 'MSTC'
	option part_number 'M02100'
	option revision 'A'
	option pn_addr	40
	option pn_len	16
	option mode	'BOSA'
	option type	'MSTC_BOSA'
	option firmware	'libpontrsv_mstc-bosa.so'
	list   capacity	'CAP_TX_POWER'
	list   capacity	'CAP_TX_DIS'
	option transform_temp		'0'
	option transform_vcc		'0'
	option transform_tx_power	'0'
	option transform_tx_bias	'0'
	option transform_tx_output	'0'
	option transform_rx_input	'0'

