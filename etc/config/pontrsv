#keep this as the last one for wildcard
config optical_transceiver 'Generic'
	option vendor_name '*'
	option part_number '*'
	option revision '*'
	option pn_addr	40
	option pn_len	16
	option mode	'DIPLEXER'
	option type	'Unknown'
	option firmware	'libpontrsv_generic.so'
	list   capacity	'CAP_TX_POWER'
	list   capacity	'CAP_TX_DIS'
	list   capacity	'CAP_DATE'
	option transform_temp		'0'
	option transform_vcc		'0'
	option transform_tx_power	'0'
	option transform_tx_bias	'0'
	option transform_tx_output	'0'
	option transform_rx_input	'1'

#be careful: the following sequence of list items should not be changed.
config trsv_mode
	list mode 'DIPLEXER'
	list mode 'TRIPLEXER'
	list mode 'BOSA'
	list mode 'TOSA'

config trsv_type
	list type 'TP_SP_1'
	list type 'DP_NP_1'
	list type 'DP_SP_1'
	list type 'MSTC_BOSA'
	list type 'TP_SP_2'
	list type 'DP_SX_1'
	list type 'DP_HS_1'

config trsv_capactity
	list capacity	'CAP_TX_POWER'
	list capacity	'CAP_RF'
	list capacity	'CAP_FW_VER'
	list capacity	'CAP_RESET'
	list capacity	'CAP_BIAS_CHANGE'
	list capacity	'CAP_LASERCURRENT_OOR_STATUS'
	list capacity	'CAP_LASERCURRENT_OOR_CONTROL'
	list capacity	'CAP_RX_SQUELCH'
	list capacity	'CAP_RX_OUTPUT'
	list capacity	'CAP_TX_DIS'
	list capacity	'CAP_DATE'

config global global
	option tx_disable '0'

