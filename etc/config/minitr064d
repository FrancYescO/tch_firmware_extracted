config minitr064d 'config'
        option enable_upnp '1'
        option log_output '0'
        option internal_iface 'lan'
        option port '0'
        option manufacturer_url 'http://www.technicolor.com/'
        option model_url 'http://www.technicolor.com/'
        option model_description 'Technicolor Internet Gateway Device'
        option https_cert '/etc/nginx/server.crt'
        option https_key '/etc/nginx/server.key'

config minitr064d 'password'
	
