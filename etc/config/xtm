config ptmdevice 'ptm0'
       option priority 'low'

config atmdevice 'atm_ppp'
        option ulp 'eth'
        option vpi '8'
        option vci '35'
        option path 'fast'
        option enc 'llc'

config atmdevice 'atm_video'
        option ulp 'eth'
        option vpi '8'
        option vci '34'
        option path 'fast'
        option enc 'llc'

config atmdevice 'atm_video2'
        option ulp 'eth'
        option vpi '9'
        option vci '89'
        option path 'fast'
        option enc 'llc'

config atmdevice 'atm_ipoe'
        option ulp 'eth'
        option vpi '8'
        option vci '33'
        option path 'fast'
        option enc 'llc'
