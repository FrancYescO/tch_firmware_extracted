config ptmdevice 'ptm0'
       option priority 'low'

config atmdevice 'atm_8_35'
        option ulp 'eth'
        option vpi '8'
        option vci '35'
        option path 'fast'
        option enc 'llc'
	option td 'td_ubr'

config trafficdesc 'td_ubr'
        option servicecat 'ubr'

config drop_threshold 'drop_threshold'
        option adsl     '256'
        option vdsl     '256'
        option vdslrtx  '256'
        option gfast    '256'

