config ptmdevice 'ptm0'
       option priority 'low'

config atmdevice 'atm0'
        option ulp 'eth'
        option vpi '8'
        option vci '35'
        option path 'fast'
        option enc 'llc'

config atmdevice 'atm1'
        option ulp 'eth'
        option vpi '8'
        option vci '36'
        option path 'fast'
        option enc 'llc'