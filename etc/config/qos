	option trafficid '7'

config label 'PPP'
	option trafficid '9'
#Qos Class Definition (queue's)

#TO_WAN
config class 'W_Q0'
	option priority '0'
	option weight '10'

config class 'W_Q1'
	list trafficid '2'
	list trafficid_tcpack '0'
	list trafficid_tcpack '1'
	option priority '0'
	option weight '10'

config class 'W_Q2'
	list trafficid '3'
	list trafficid '4'
	list trafficid_tcpack '2'
	option priority '0'
	option weight '30'
	option swprio '1'

config class 'W_Q3'
	list trafficid '6'
	list trafficid_tcpack '3'
	list trafficid_tcpack '4'
	option priority '0'
	option weight '50'
	option swprio '1'

config class 'W_Q4'
	option priority '4'
	option swprio '2'

config class 'W_Q5'
	list trafficid '5'
	list trafficid_tcpack '5'
	list trafficid_tcpack '6'
	option priority '5'
	option swprio '3'

config class 'W_Q6'
	list trafficid '8'
	option priority '6'
	option swprio '4'

config class 'W_Q7'
	list trafficid '9'
	list trafficid '7'
	option priority '7'
	option swprio '5'

#TO_LAN
config class 'L_Q0'
	option priority '0'

config class 'L_Q1'
	list trafficid '4'
	option priority '1'

config class 'L_Q2'
	list trafficid '5'
	list trafficid '6'
	list trafficid '7'
	option priority '2'

config class 'L_Q3'
	list trafficid '8'
	option priority '3'

#TO_WLAN
config class 'WMM_BK_Q0'
	option priority '0'

config class 'WMM_BK_Q1'
	option priority '1'

config class 'WMM_BE_Q0'
	option priority '2'

config class 'WMM_BE_Q1'
	option priority '3'

config class 'WMM_VI_Q0'
	list trafficid '4'
	option priority '4'

config class 'WMM_VI_Q1'
	list trafficid '5'
	option priority '5'

config class 'WMM_VO_Q0'
	list trafficid '6'
	list trafficid '7'
	option priority '6'

config class 'WMM_VO_Q1'
	list trafficid '8'
	option priority '7'

#Qos Classgroup Definition
config classgroup 'TO_WAN'
	option classes 'W_Q0 W_Q1 W_Q2 W_Q3 W_Q4 W_Q5 W_Q6 W_Q7'
	option default 'W_Q0'
	option policy 'sp_wrr'

config classgroup 'TO_LAN'
	option classes 'L_Q0 L_Q1 L_Q2 L_Q3'
	option default 'L_Q0'
	option policy 'sp'

config classgroup 'TO_WLAN'
	option classes 'WMM_BE_Q0 WMM_BK_Q0 WMM_BK_Q1 WMM_BE_Q1 WMM_VI_Q0 WMM_VI_Q1 WMM_VO_Q0 WMM_VO_Q1'
	option default 'WMM_BE_Q0'
	option policy 'sp'

# Since QoSv2.1 it's preferred to set classgroup on device level
config device 'atm_8_35'
	option classgroup 'TO_WAN'

config device 'ptm0'
	option classgroup 'TO_WAN'
	option swshaper 'vdsl_swshaper'

config swshaper 'vdsl_swshaper'
	option enable '0'
	option max_bit_rate '40000'

config device 'eth4'
	option classgroup 'TO_WAN'

config device 'eth0'
	option classgroup 'TO_LAN'

config device 'eth1'
	option classgroup 'TO_LAN'

config device 'eth2'
	option classgroup 'TO_LAN'

config device 'eth3'
	option classgroup 'TO_LAN'

config device 'wl0'
	option classgroup 'TO_WLAN'

config device 'wl1'
	option classgroup 'TO_WLAN'

#QoS classification rules (only IPv4 classification present due to lack of IPv6 configuration plane)
#IMPORTANT NOTE: Make sure that the order in which the rules are defined is from Lowest Priority to Highest Priority!

config reclassify 'reclassify_1'
	option order '0'
	option target 'dscp_Zero'
	list srcif 'lan'
	list srcif 'Guest1'
	list srcif 'Guest1_5GHz'
	list srcif 'fonopen'
	list dstif 'wan'
	list dstif 'wan6'

config reclassify 'reclassify_2'
	option order '0'
	option target 'dscp_Zero'
	list dscp '!EF'
	list dstif 'wan'
	list dstif 'wan6'

config classify 'classify_1'
	option target 'FON'
	option proto 'udp'
	option dstports '1701'

config classify 'classify_2'
	option target 'LoT'
	option dstports '5683'
	option proto 'udp'

