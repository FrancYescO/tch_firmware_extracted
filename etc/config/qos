#Internal Classification Labels

config label 'Best_Effort'
	option dscp '0'

config label 'Normal'
	option trafficid '1'

config label 'User'
	option trafficid '2'

config label 'Video'
	option trafficid '3'

config label 'DNS'
	option trafficid '4'

config label 'Management'
	option trafficid '5'

config label 'DHCP'
	option trafficid '6'

config label 'Voice'
	option trafficid '7'

config label 'PPP'
	option trafficid '8'

config label 'CWMP'
        option trafficid '9'

config label 'pcp_5'
        option pcp '5'

#Qos Class Definition (queue's)

#TO_WAN
config class 'W_Q0'
	option weight '10'
	option priority '0'

config class 'W_Q1'
	list trafficid '2'
	option weight '10'
	option priority '0'

config class 'W_Q2'
	list trafficid '3'
	option weight '10'
	option priority '0'

config class 'W_Q3'
	list trafficid '4'
	option weight '10'
	option priority '0'

config class 'W_Q4'
	list trafficid '5'
	option weight '40'
	option priority '0'

config class 'W_Q5'
	list trafficid '6'
	list trafficid '9'
	option priority '5'

config class 'W_Q6'
	list trafficid '7'
	option priority '6'

config class 'W_Q7'
	list trafficid '8'
	option priority '7'

#TO_LAN
config class 'L_Q0'
	option priority '0'

config class 'L_Q1'
	list trafficid '3'
	option priority '1'

config class 'L_Q2'
	list trafficid '4'
	list trafficid '5'
	list trafficid '6'
	list trafficid '9'
	option priority '2'

config class 'L_Q3'
	list trafficid '7'
	option priority '3'

#TO_WLAN
config class 'WMM_BK_Q0'
	option priority '0'

config class 'WMM_BK_Q1'
	option priority '1'

config class 'WMM_BE_Q0'
	option priority '2'

config class 'WMM_BE_Q1'
	option priority '3'

config class 'WMM_VI_Q0'
	list trafficid '3'
	option priority '4'

config class 'WMM_VI_Q1'
	list trafficid '4'
	option priority '5'

config class 'WMM_VO_Q0'
	list trafficid '5'
	list trafficid '6'
	list trafficid '9'
	option priority '6'

config class 'WMM_VO_Q1'
	list trafficid '7'
	option priority '7'

#Qos Classgroup Definition
config classgroup 'TO_WAN'
	option classes 'W_Q0 W_Q1 W_Q2 W_Q3 W_Q4 W_Q5 W_Q6 W_Q7'
	option default 'W_Q0'
	option policy 'sp_wrr'

config classgroup 'TO_LAN'
	option classes 'L_Q0 L_Q1 L_Q2 L_Q3'
	option default 'L_Q0'
	option policy 'sp'

config classgroup 'TO_WLAN'
	option classes 'WMM_BE_Q0 WMM_BK_Q0 WMM_BK_Q1 WMM_BE_Q1 WMM_VI_Q0 WMM_VI_Q1 WMM_VO_Q0 WMM_VO_Q1'
	option default 'WMM_BE_Q0'
	option policy 'sp'

config classgroup 'TO_GUEST'
	option classes 'WMM_BE_Q0 WMM_BK_Q0'
	option default 'WMM_BK_Q0'
	option policy 'sp'

# LAN
config interface 'lan'
	option classgroup 'TO_LAN'
	option enable '1'

# WAN
config interface 'wan'
	option classgroup 'TO_WAN'
	option enable '1'

#To correctly configure queues of vdsl vlan
config device 'ptm0'
	option classgroup 'TO_WAN'
	option enable '1'

#To correctly configure the shaper of eth vlan
config device 'eth4'
	option classgroup 'TO_WAN'
	option enable '1'

#WLAN
config device 'wl0'
	option classgroup 'TO_WLAN'
	option enable '1'

config device 'wl0_1'
	option classgroup 'TO_GUEST'
	option enable '1'

config device 'wl1'
	option classgroup 'TO_WLAN'
	option enable '1'

config device 'wl1_1'
	option classgroup 'TO_GUEST'
	option enable '1'

#QoS classification rules (only IPv4 classification present due to lack of IPv6 configuration plane)
#IMPORTANT NOTE: Make sure that the order in which the rules are defined is from Lowest Priority to Highest Priority!

# Rules with dstif are applied after rules without dstif, effectively overruling them
config reclassify 'reclassify_1'
	option target 'Voice'
	list srcif 'loopback'
	list dscp 'EF'

config reclassify 'reclassify_2'
	option target 'Best_Effort'
	list srcif 'lan'
	list srcif 'Guest1'
	list srcif 'Guest1_5GHz'
	list dstif 'wan'

config reclassify 'reclassify_3'
	option target 'Best_Effort'
	list dscp '!EF'
	list dstif 'wan'

config classify 'classify_4'
	option target 'User'
	option proto 'tcp'
	option dstports '20,21,25,80,109,110,143,201,202,203,204,205'

config classify 'classify_5'
	option target 'User'
	option proto 'tcp'
	option dstports '206,220,387,587,993,995,1723,1935,3074,3658,8080'

config classify 'classify_6'
	option target 'User'
	option proto 'tcp'
	list srcif 'lan'
	list srcif 'Guest1'
	list srcif 'Guest1_5GHz'
	option dstports '443'

config classify 'classify_7'
	option target 'User'
	option proto 'ah'

config classify 'classify_8'
	option target 'User'
	option proto 'udp'
	option dstports '123'

config classify 'classify_9'
	option target 'User'
	option dstports '201,202,203,204,205,206,220,387'
	option proto 'udp'

config classify 'classify_10'
	option target 'User'
	option dstports '993,995,1935,3478,3479,3480'
	option proto 'tcp'

config classify 'classify_11'
	option target 'User'
	option dstports '1701,3478,3479,3480'
	option proto 'udp'

config classify 'classify_12'
	option target 'Video'
	option proto 'tcp'
	option dstports '554'

config reclassify 'reclassify_13'
	option target 'Video'
	option helper 'rtsp'

config classify 'classify_14'
	option target 'Video'
	option proto 'igmp'

config classify 'classify_15'
	option target 'DNS'
	option dstports '53'
	option proto 'tcp'

