config dnsmasq 'main'
	option disabled			 '0'
	option nonwildcard		 '1'
	option domainneeded		 '1'
	option boguspriv		 '1'
	option filterwin2k		 '0'
	option localise_queries	 '1'
	option rebind_protection '0'
	option rebind_localhost	 '1'
	option local			 '/lan/'
	option expandhosts		 '1'
	option nonegcache		 '0'
	option authoritative	 '1'
	option readethers		 '1'
	option leasefile		 '/tmp/dhcp.leases'
	option resolvfile		 '/tmp/resolv.conf.auto'
	option sticky_servers    '1'
	option strictorder		 '1'
	option dhcpscript		 '/lib/dnsmasq/dhcp-event.sh'
	option domain			 'lan'
	list   hostname			 'dsldevice'
	list   interface		 'lan'
	list   interface		 'Guest1'
	list   interface		 'Guest1_5GHz'

config odhcpd 'odhcpd'
	option leasefile	 '/tmp/hosts/odhcpd'
	option leasetrigger	 '/usr/sbin/odhcpd-update'

config dhcp 'lan'
	option instance	     'main'
	option interface	 'lan'
	option start		 '2'
	option limit		 '253'
	option leasetime	 '24h'
	option force		 '1'
	option dhcpv6		 'server'
	option ra		 'server'
	option ra_management	 '0'
	option ra_mininterval	 '200'
	option ra_maxinterval	 '600'
	option ra_lifetime	 '1800'
	option ra_hoplimit	 '64'
	option ra_mtu	     '1500'
	list   dhcp_option	 '6,10.1.1.1'

config dhcp 'Guest1_private'
	option instance	     'main'
	option interface	 'Guest1'
	option start		 '2'
	option limit		 '253'
	option leasetime	 '24h'
	option force		 '1'
	option dhcpv6		 'server'
	option ra		     'server'
	option ra_management	 '0'
	option ra_mininterval	 '200'
	option ra_maxinterval	 '600'
	option ra_lifetime	 '1800'
	option ra_hoplimit	 '64'
	option ra_mtu	     '1500'
	list   dhcp_option	 '6,10.1.2.1'

config dhcp 'Guest1_5GHz_private'
	option instance	    'main'
	option interface	 'Guest1_5GHz'
	option start		 '2'
	option limit		 '253'
	option leasetime	 '24h'
	option force		 '1'
	option dhcpv6		 'server'
	option ra		     'server'
	option ra_management	 '0'
	option ra_mininterval	 '200'
	option ra_maxinterval	 '600'
	option ra_lifetime	 '1800'
	option ra_hoplimit	 '64'
	option ra_mtu   	 '1500'
	list   dhcp_option	 '6,10.1.3.1'

config dhcp 'wan'
	option interface	 'wan'
	option ignore		 '1'

config dhcp 'ppp'
	option interface	 'ppp'
	option ignore		 '1'

config dhcp 'pppv'
	option interface	 'pppv'
	option ignore		 '1'

config dhcp 'ipoe'
	option interface	 'ipoe'
	option ignore		 '1'

config dhcp 'wan6'
	option interface	 'wan6'
	option ignore		 '1'

config dhcp 'wwan'
	option interface	 'wwan'
	option ignore		 '1'

config dhcp 'VLAN1'
	option interface	 'VLAN1'
	option ignore		 '1'

config dhcp 'VLAN2'
	option interface	 'VLAN2'
	option ignore		 '1'

config dhcp 'VLAN3'
	option interface	 'VLAN3'
