config init 'multiap_agent'
	option state 'uninitialised'
	option enabled '0'
	option readloop '500'
	option pollloop '3000'
	option eventloop '5000'
	option macaddress ''

config 1905_config 'al_entity'
        option enabled '0'
        option al_mac ''
        option interfaces 'eth0,eth1,eth2,eth3,wl0,wl1'

config init 'multiap_controller'
	option state 'uninitialised'
	option enabled '0'
	option readloop '500'
	option eventloop '5000'
	option macaddress ''

config capabilities 'controller_capabilities'
	option freq_2_4g '1'
	option freq_5g '1'
	option freq_60g '0'

config interfaces 'agent_interfaces'
	list iface 'wl0'

config interfaces 'agent_ap'
	list wifi_ap 'ap0 ap1 ap2'

config fh_config 'fronthaul'
	option interface ''

config bh_config 'backhaul'
	option interface ''

