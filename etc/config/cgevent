config control 'iperf_mem_oom'
       option register_file '/cgroups/cpumemblk/iperf/memory.oom_control'
       list exe_script '/lib/cgevent/iperf'

config control 'iperf_mem_usage'
       option register_file '/cgroups/cpumemblk/iperf/memory.usage_in_bytes'
       option threhold '20M'
       list exe_script '/lib/cgevent/iperf'
