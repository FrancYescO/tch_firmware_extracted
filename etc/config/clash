config single_config 'main_config'
	option module_path '/usr/lib/lua/clash/modules'
	option log_level '3'

config user 'engineer'
	option ssh '1'
	option telnet '0'
	option serial '0'
	option gak_id '1'
	option historyfile '/etc/clash/historyengineer.txt'
	list command 'add'
	list command 'apply'
	list command 'brctl'
	list command 'coredump'
	list command 'count'
	list command 'del'
	list command 'df'
	list command 'dmesg'
	list command 'dsldiagd'
	list command 'get'
	list command 'getpn'
	list command 'ifconfig'
	list command 'ifstat'
	list command 'ifstatus'
	list command 'ip'
	list command 'iptables'
	list command 'ip6tables'
	list command 'list'
	list command 'lock'
	list command 'logread'
	list command 'newsrpuser'
	list command 'nslookup'
	list command 'ping'
	list command 'ps'
	list command 'readlogfile'
	list command 'reboot'
	list command 'resolve'
	list command 'route'
	list command 'rtfd'
	list command 'set'
	list command 'showinfo'
	list command 'showtag'
	list command 'subscribe'
	list command 'tcpdump'
	list command 'top'
	list command 'traceroute'
	list command 'ubuscall'
	list command 'ubuslisten'
	list command 'unlock'
	list command 'unsubscribe'
	list command 'upgrade'
	list command 'uptime'
	list command 'wireless_autochannel'
	list command 'xdslctl'

config user 'superuser'
	option ssh '1'
	option telnet '0'
	option serial '0'
	option gak_id '1'
	option historyfile '/etc/clash/historysuperuser.txt'
	list command 'lock'
	list command 'reboot'
	list command 'showtag'
	list command 'unlock'
