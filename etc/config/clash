config single_config 'main_config'
	option module_path '/usr/lib/lua/clash/modules'
	option log_level '6'

config user 'lanadmin'
	option ssh '0'
	option serial '1' 
	option telnet '1'
	option gak_id '0'
	option historyfile '/etc/clash/historytechnician.txt'
	list command 'add'
	list command 'apply'
	list command 'count'
	list command 'del'
	list command 'dmesg'
	list command 'dsldiagd'
	list command 'get'
	list command 'getpn'
	list command 'ifconfig'
	list command 'ifstatus'
	list command 'ip'
	list command 'iptables'
	list command 'list'
	list command 'logread'
	list command 'nslookup'
	list command 'ping'
	list command 'ps'
	list command 'readlogfile'
	list command 'reboot'
	list command 'resolve'
	list command 'route'
	list command 'rtfd'
	list command 'set'
	list command 'subscribe'
	list command 'tcpdump'
	list command 'top'
	list command 'traceroute'
