config tod 'global'
        option enabled '1'
        option tod_enabled '1'
        option tracelevel '6'
        option time_change_monfreq '10'

config timer 'led_timer'
        option start_time 'All:22:00'
        option stop_time 'All:06:30'

config action 'led_action'
        option enabled '0'
        list timers 'led_timer'
        option script 'ledtodscript'

config timer 'guest_restriction_timer'
        option periodic '0'

config action 'guest_restriction_action'
        option enabled '1'
        list timers 'guest_restriction_timer'
        option script 'guestrestriction'
        option object 'all'
