config global 'global'
    option interface 'wan'
    option interface6 'wan6'
    option mode 'BFD'
    option enabled '1'

config bfdecho 'bfdecho_config'
    option poll_interval '30'
    option failed_limit '4'
    option timeout '1'
    option ipv4_enabled '1'
    option ipv6_enabled '1'
    option delay '25'
    option dscp AF21

config dns 'dns_config'
    option poll_interval '300'
    option failed_limit '4'
    option timeout '5'
    option retry_interval '5'
    option ipv4_enabled '1'
    option ipv6_enabled '1'
    option domain_name 'fbbwan.telstra.net'
    option delay '0'
    option dnsmasq_cfg 'main'

config heartbeat 'heartbeat_config'
    option poll_interval '300'
    option timeout '5'
    option ipv4_enabled '1'
    option ipv6_enabled '1'
    option domain_name 'fbbwan.telstra.net'
