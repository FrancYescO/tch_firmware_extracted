config syslog 'syslog'
	option trace '0'

config onboarding 'onboarding'
	option enable '0'

config wps_status 'wps_status'
	option timeout '300'

config rssi_status_off 'rssi_status_off'
	option enable '0'
	option timeout '60'

config easymesh_status 'easymesh_status'
	option enable '1'

config brightness 'wps'
	option name 'wps'
	option red '7'
	option green '4'
	option orange '8'

config rssi_values 'rssi_values'
	option too_good '-45'
	option good '-62'
	option bad '-72'
