
config main 'main'
  option devname 'TECTEL01'
  option iface 'br-fonopen'
  option iface_eap 'bla'
  option ifb 'ifb0'
  option ifb2 'ifb1'
  option iface_priv 'br-lan'
  option ipv4 '1'
  option ipv6 '0'
  option enable 'true'
  option deploy 'true'
  option nasid 'SET_BY_SCRIPT'
  option preauth '1'
  option bandwidth_threshold '8000'
  option bandwidth_hysteresis '500'
  option location_name 'FonTEL:AU'

config wifi-iface 'TLS2G'
  option enable '1'
  option iface 'wl0_2'

config wifi-iface 'FON2G'
  option enable '1'
  option iface 'wl0_3'

config wifi-iface 'FON5G'
  option enable '0'
  option iface 'wl1_3'

config client 'client'
  option limit '64'
  option auth_limit '1'
  option preauth_limit '4'
  option tunnel_backend 'ppp'
  option idle_max '1200'
  option timer '15'
  option auth 'ppp'
  option trigger_newuser '/usr/sbin/hotspot-user'
  option trigger_deluser '/usr/sbin/hotspot-user'

config redirect 'redirect'
  option maxconn '32'
  option port '3989'
  option url 'https://telstra.portal.fon.com/jcp/telstra'
  option templateurl 'https://telstra.portal.fon.com/jcp/telstra'

config portal 'portal'
  option maxconn '32'
  option port '3990'
  option secret 'garrafon'

config firewall 'firewall'
  option hammer_times '20'
  option services_udp '53 67'
  option ipv4_forward_chain 'FORWARD'
  option ipv6_forward_chain 'FORWARD'
  option ipv4_input_chain 'INPUT'
  option ipv6_input_chain 'INPUT'
  option ipv4_output_chain 'OUTPUT'
  option ipv6_output_chain 'OUTPUT'
  option ipv4_prerouting_chain 'PREROUTING'
  option ipv6_postrouting_chain 'POSTROUTING'
  option anydns '1'

config traffic 'traffic'
  option overall_kbit_in '0'
  option overall_kbit_out '0'
  option shaping 'no'

config whitelist 'whitelist'
  option timeout '600'
  list static 'telstra.portal.fon.com'
