config single_config 'global'
        option enable '0'
        list configurations 'config1'
        list configurations 'config2'
        list configurations 'config3'

config configuration 'config1'
        option type '1731'
        option mepId '2'
        option megLevel '2'
        option ifname 'ptm0'
        option meg 'MEG1'
        option ccm "0"
        option CCMinterval "5"

config configuration 'config2'
        option type '1731-2'
        option mepId '3'
        option megLevel '3'
        option ifname 'ptm0'
        option meg 'MEG2'
        option ccm "0"
        option CCMinterval "5"

config configuration 'config3'
        option type '3ah'
        option ifname 'ptm0'
        option oamID '2'
        option features '+vr+lb+le'
