config mqttjson_services 'config'
	option enabled '0'
        option tls_enabled '0'
        option tls_certs '/etc/ssl/certs:/proc/rip/011a.cert:/proc/keys/0x11A'
        option tls_keyform 'engine:keystore'
	option cert_location '/etc/mqttjson_services/certs/'
	option connection_url 'a3b2gvd522o1rc.iot.us-east-1.amazonaws.com'
	option connection_port '8883'
        option log_level '5'
        option ui_version 'UI-2.0'
        option customer_name 'Technicolor'
        option customer_name_enabled '0'
        option service_id_enabled '1'
        option ignore_physical_auth '0'
	option backoff_minwait '5'
	option backoff_multiplier '2000'
	option register '0'
        list req_whitelist 'rpc.network.firewall.portforward.'
        list req_whitelist 'InternetGatewayDevice.LANDevice.'
        list req_whitelist 'rpc.wireless.ssid.'
        list req_whitelist 'uci.wireless.wifi-iface.'
        list req_whitelist 'rpc.wireless.radio.'
        list req_whitelist 'rpc.wireless.ap.'
        list req_whitelist 'uci.wireless.wifi-bandsteer.'
        list req_whitelist 'rpc.wireless.wps_button'
        list req_whitelist 'rpc.mmpbx.calllog.clear'
        list req_whitelist 'sys.hosts.host.'
        list req_whitelist 'uci.dhcp.host.'
        list req_whitelist 'uci.parental.URLfilter.'
        list req_whitelist 'uci.tod.host.'
        list req_whitelist 'rpc.network.firewall.mode'
        list req_whitelist 'uci.parental.general.enable'
        list req_whitelist 'InternetGatewayDevice.User.'
        list req_whitelist 'InternetGatewayDevice.X_000E50_OoklaSpeedTest.'
        list req_whitelist 'uci.tod.ap.'
        list req_whitelist 'uci.tod.wifitod.'
        list req_whitelist 'uci.tod.action.'
        list req_whitelist 'uci.tod.timer.'
        list req_whitelist 'uci delete tod.'
        list req_whitelist 'uci add_list tod.'
        list req_whitelist 'uci commit'
        list req_whitelist '/etc/init.d/firewall reload'
        list req_whitelist '/etc/init.d/tod reload'
        list req_whitelist 'reboot'
        list req_whitelist 'echo -n > /etc/cwmpd.db; /etc/init.d/cwmpd restart'
        list rsp_whitelist 'uci.network.interface.'
        list rsp_whitelist 'uci.network.device.'
        list rsp_whitelist 'rpc.network.interface.'
        list rsp_whitelist 'rpc.network.firewall.'
        list rsp_whitelist 'uci.parental.URLfilter.'
        list rsp_whitelist 'uci.multiap.controller.'
	list rsp_whitelist 'uci.multiap.agent.'
        list rsp_whitelist 'rpc.multiap.device.'
        list rsp_whitelist 'sys.hosts.host.'
        list rsp_whitelist 'rpc.hosts.host.'
        list rsp_whitelist 'uci.dhcp.host.'
        list rsp_whitelist 'rpc.wireless.ssid.'
        list rsp_whitelist 'rpc.wireless.ap.'
        list rsp_whitelist 'rpc.wireless.radio.'
        list rsp_whitelist 'uci.wireless.wifi-iface.'
        list rsp_whitelist 'uci.wireless.wifi-bandsteer.'
        list rsp_whitelist 'uci.tod.ap.'
        list rsp_whitelist 'uci.tod.action.'
        list rsp_whitelist 'uci.tod.timer.'
        list rsp_whitelist 'uci.tod.host.'
        list rsp_whitelist 'rpc.mmpbx.calllog.info.'
        list rsp_whitelist 'rpc.mmpbx.sip_profile.'
        list rsp_whitelist 'rpc.mmpbx.profile.'
        list rsp_whitelist 'InternetGatewayDevice.User.'
        list rsp_whitelist 'InternetGatewayDevice.LANDevice.'
        list rsp_whitelist 'InternetGatewayDevice.X_000E50_OoklaSpeedTest.'
        list rsp_whitelist 'rpc.multiap.'
        list rsp_whitelist 'uci.cwmpd.cwmpd_config.'
        list rsp_whitelist 'uci.env.var.'
        list rsp_whitelist 'uci.env.rip.'
        list rsp_whitelist 'rpc.system.'
        list rsp_whitelist 'sys.mem.'
        list rsp_whitelist 'sys.proc.'
        list rsp_whitelist 'sys.time.'
        list rsp_whitelist 'sys.class.net.'

config add_new_extender 'add_new_extender'
        option interval '5000'
        option timeout '120000'
