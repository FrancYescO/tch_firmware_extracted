#NG-65290, NG-73365
config system
	option hostname	'modemtim'
	option zonename	'Europe/Rome'
	option timezone	'CET-1CEST,M3.5.0,M10.5.0/3'
	option network_timezone 0
#NG-102268 Enlarge the default log buffer size
	option log_buffer_size '4096'
	
config timeserver ntp
	list server	ntp-tr069-1.interbusiness.it
	list server	ntp-tr069-2.interbusiness.it
	list server	ntp1.inrim.it
	option enable_server 1

config config config
	option export_plaintext	0
	option export_unsigned	0
#NG-73365 parameter changed to 0
	option import_plaintext	0
	option import_unsigned	0
	option import_restricted 1
	option enable_usb3_support '0'
	option enable_usb2_support '1'
	option enable_usb1_support '1'

config coredump
	option reboot	'1'
	option action	'compress'
	option path	'/root'

config log logread
	option path 'logread'

#Backup and restore support between different releases
config importversion
	option version_match 'AGTHP_%d%.%d.%d[_%d]*'
	option importer '/etc/import/import_ti.lua'
	
