config system
	option hostname	OpenWrt
	option conloglevel 1
	option filterloglevel '8'
	option log_buffer_size '2048'
	option timezone 'GMT0BST,M3.5.0/1,M10.5.0'
	option network_timezone 1
	option zonename Europe/London
	option hostname dsldevice
	option log_prefix "set_by_script"

config timeserver ntp
	list server	ntp-tr069-1.interbusiness.it
	list server	ntp-tr069-2.interbusiness.it
	option enabled 1
	option use_dhcp 1
	option enable_server 1

config config config
	option export_plaintext	0
	option export_unsigned	0
	option import_plaintext	1
	option import_unsigned	0
	option export_commonkey 'GW'
	option import_commonkey 'GW'
	option import_restricted 1
	option enable_usb1_support 0
	option enable_usb2_support 0
	option enable_usb3_support 0

config coredump
	option reboot	'1'
	list reboot_exceptions 'dnsmasq'
	option action	'compress'
	option path	'/root'

config kernel_crash
	option action	'compress'
	option path	'/root'

config log logread
	option path 'logread'
	option size '0'
	option rotate '0'
