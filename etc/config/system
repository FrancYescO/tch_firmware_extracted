config system
	option hostname	mydevice
	option timezone 'NZST-12NZDT,M9.5.0,M4.1.0/3'
	option zonename 'Pacific/Auckland'
	option network_timezone 0

config timeserver ntp
	list server	0.nz.pool.ntp.org
	list server	1.nz.pool.ntp.org
	list server	2.nz.pool.ntp.org
	option enable_server 1

config config config
	option export_plaintext	0
	option export_unsigned	0
	option import_plaintext	1
	option import_unsigned	0

config coredump
	option reboot	'1'
	option action	'compress'
	option path	'/root'

config log logread
	option path 'logread'
