config system
	option hostname	OpenWrt
	option timezone	'CET-1CEST,M3.5.0,M10.5.0/3'
	option gui_timezonename 'CET-1CEST,M3.5.0,M10.5.0/3_timezone_Europe/Rome'
	option zonename 'Europe/Rome'
	option network_timezone 1
	option log_buffer_size '64'
	option log_size '1000'
	option log_file '/custo/syslog'

config timeserver ntp
        list server	0.openwrt.pool.ntp.org
        list server	1.openwrt.pool.ntp.org
	option enable_server 1

config config config
	option export_plaintext	0
	option export_unsigned	0
	option import_plaintext	1
	option import_unsigned	0
	option iad_function 1

config coredump
	option reboot	'1'
	option action	'compress'
	option path	'/root'

config kernel_crash
	option action	'compress'
	option path	'/root'

config log logread
	option path 'logread'

config speedtest 'speedtest'
	option enabled '1'
	option run_in_lxc '0'
