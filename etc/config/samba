config samba
        option 'name'                   'MyRepublic Wi-Fi Hub'
        option 'workgroup'              'WORKGROUP'
        option 'description'            'MyRepublic Wi-Fi Hub'
        option 'homes'                  '1'
        option 'enabled'                '1'
        option 'charset'                'UTF-8'

config 'sambashare'
        option 'name' 'printers'
        option 'path' '/var/spool/samba'
        option 'guest_ok' 'yes'
        option 'printable' 'yes'
        option 'browsable' 'no'
        option 'writable' 'no'
        option 'enabled' '0'
        option 'min_print_space' '10000'
