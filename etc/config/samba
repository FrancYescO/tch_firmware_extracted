config samba
        option 'name'                   'Technicolor'
        option 'workgroup'              'WORKGROUP'
        option 'description'            'Technicolor Gateway'
        option 'homes'                  '1'
        option 'enabled'                '1'
        option 'charset'                'UTF-8'

config 'sambashare'
        option 'name' 'usbdisk'
        option 'path' '/mnt/usb'
        option 'guest_ok' 'yes'
        option 'create_mask' '0700'
        option 'dir_mask' '0700'
        option 'read_only' 'no'
        option 'enabled' '1'

config 'sambashare'
        option 'name' 'printers'
        option 'path' '/var/spool/samba'
        option 'guest_ok' 'yes'
        option 'printable' 'yes'
        option 'browsable' 'no'
        option 'writable' 'no'
        option 'enabled' '1'
        option 'min_print_space' '0'
