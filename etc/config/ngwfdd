config ngwfdd config
    option fifo_dir '/tmp/gwfd'
    option flush_size '30'
    option enable '0'
    option flush_interval '900'
    option base_url ''
    option tag ''
    option capath '/etc/ssl/certs/'

config collector devices
    option enabled '1'
    option interval '3600'

config collector cpustats
    option enabled '1'
    option interval '600'

config collector meminfo
    option enabled '1'
    option interval '300'

config collector slabinfo
    option enabled '1'
    option interval '300'

config collector process
    option enabled '1'
    option interval '300'

config collector lte
    option enabled '1'
    option interval '300'

config collector dsl
    option enabled '1'
    option interval '1800'

config collector wan
    option enabled '1'
    option interval '1800'

config collector video
    option enabled '1'
    option interval '1800'

config collector conntrack
    option enabled '1'
    option interval '1800'

config collector fhcd
    option enabled '0'

config collector 'dev_connected'
    option enabled '1'
    option interval '3600'
