config dlnad 'config'
	option enabled '0'
	option manufacturer_url 'http://www.technicolor.com'
	option model_url 'http://www.technicolor.com'
	option radioStations_enabled '0'
	option interface 'br-lan'
	option friendly_name 'OpenWrt DLNA Server'
	list interface 'lan'

config global 'log'
	option dlnad_logLevel '2'
	option mud_logLevel '2'
	option mvfs_logLevel '2'
