    option minimum_attach_delay '0'
    option maximum_attach_delay '10'

config platform 'platform'
    option power_on '1'
    option antenna 'auto'

config profile
    option id '1'
    option visible '1'
    option editable '0'
    option name 'default'
    option apn ''
    option pdptype 'ipv6'

