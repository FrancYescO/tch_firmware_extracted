config bulkdata 'global'
    option enabled '1'
    option tracelevel '0'
    option min_report_interval '60'
    option protocols 'HTTP'
    option encode_types 'JSON'
    option wild_card '1'
    option max_num_profiles '10'
    option max_num_parameters '100'

config profile 'profile_1'
        option enabled '0'
        option alias 'cpe-123456'
