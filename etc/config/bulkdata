config bulkdata 'global'
        option enabled '0'
        option tracelevel '0'
        option min_report_interval '60'
        option protocols 'HTTP'
        option encode_types 'JSON'
        option wild_card '1'
        option max_num_profiles '10'
        option max_num_parameters '100'

config profile 'profile_1'
        option enabled '0'
        option alias 'cpe-123456'
        option name 'profile1'
        option retain_failed_reports '1'
        option protocol 'HTTP'
        option encode_type 'JSON'
        option report_interval '86400'
        option time_reference '0001-01-01T00:00:00Z'
        option json_report_format 'ObjectHierarchy'
        option json_report_timestamp 'Unix-Epoch'
        option http_url 'http://abc.com'
        option http_username '1234'
        option http_password '1234'
        option http_compressions_supported 'GZIP'
        option http_compression 'GZIP'
        option http_methods_supported 'POST'
        option http_method 'POST'
        option http_usedate_header '1'
        option retry_enable '1'
        option retry_min_interval '5'
        option retry_interval_multiplier '2000'
        list http_uri 'Device.Time.CurrentLocalTime|ct'
        list parameters 'Device.DeviceInfo.UpTime|DeviceDeviceInfoUpTime'
