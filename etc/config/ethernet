config globals globals
        option trafficmanager '1'
        option multicastlearning '1'

config port 'eth0'
        option enable '1'
        option speed 'auto'
        option duplex 'full'
        option pauseenable '0'
        option softswitch '0'

config port 'eth1'
        option enable '1'
        option speed 'auto'
        option duplex 'full'
        option pauseenable '0'
        option softswitch '0'

config port 'eth2'
        option enable '1'
        option speed 'auto'
        option duplex 'full'
        option pauseenable '0'
        option softswitch '0'

config port 'eth3'
        option enable '1'
        option speed 'auto'
        option duplex 'full'
        option pauseenable '0'
        option softswitch '0'

config port 'veip0'
        option enable '1'
        option wan '1'
        option td 'shapingconf'

config trafficdesc 'shapingconf'
       option enable '0'
