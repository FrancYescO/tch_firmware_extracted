config global 'globals'
        option trafficmanager '1'
        option multicastlearning '1'
        option eth4lanwanmode '0'

config port 'eth0'
        option enable '1'
        option speed 'auto'
        option duplex 'full'
        option pauseenable '0'
        option softswitch '0'

config port 'eth1'
        option enable '1'
        option speed 'auto'
        option duplex 'full'
        option wan '0'
        option pauseenable '0'
        option softswitch '0'
