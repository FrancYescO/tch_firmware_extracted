config dropbear 'lan'
	option enable '0'
	option Interface 'lan'
	option PasswordAuth 'on'
	option RootLogin '0'
	option RootPasswordAuth '0'
	option Port '22'
	option IdleTimeout '600'
	option AllowLocalForwarding '0'

config dropbear 'public_lan'
	option enable '0'
	option Interface 'public_lan'
	option PasswordAuth 'on'
	option RootPasswordAuth '0'
	option Port '22'
	option IdleTimeout '600'
	option RootLogin '0'
	option AllowLocalForwarding '0'

config dropbear 'wan'
	option enable '0'
	option Interface 'wan'
	option PasswordAuth 'on'
	option RootPasswordAuth '0'
	option Port '22'
	option IdleTimeout '600'
	option RootLogin '0'
	option AllowLocalForwarding '0'
