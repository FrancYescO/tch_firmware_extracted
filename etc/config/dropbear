config dropbear 'lan'
	option enable '0'
	option IdleTimeout	'600'	
	option PasswordAuth 'on'
	option Port         '22'
	option Interface 'lan'		
#	option BannerFile   '/etc/banner'

config dropbear 'wan'
    option enable           '0'
	option IdleTimeout		'600'	
    option PasswordAuth     'on'
    option Port             '22'
    option Interface        'wan'
