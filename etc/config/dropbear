#NG-88913

config dropbear 'lan'
	option enable '0'
	option IdleTimeout	'600'	
	option PasswordAuth 'on'
	option Port '22'
	option Interface 'lan'

config dropbear 'wan'
	option enable '0'
	option Interface 'wan'
	option PasswordAuth 'on'
	option RootPasswordAuth 'on'
	option Port '22'
	option IdleTimeout '600'

