# NG-78587 chenged global.no_answer_timeout and call waiting timeout
# NG-161899 mmpbx config TI not lineup with generic mmpx config

#/*******************************************************************/
#/*                   global configuration                          */
#/*******************************************************************/
config mmpbx 'global'
	option trace_level '2'
	option enabled '1'
	option cw_reject_scope 'global'
	option country 'italyti'
	option fall_back_dialing '1'
	option no_answer_timeout '1200'
	option dial_timeout '20'

config dectemission 'dectemission'
        option state '1'

#/*******************************************************************/
#/*                   syslog configuration                          */
#/*******************************************************************/
config syslog 'syslog'
	option service_config '1'
	option service_actions '1'
	option calls '1'
	option syslog_priority '6'
	option hide_user_identity '0'

#/************************************************************************/
#/*                   diagnostics configuration                          */
#/************************************************************************/
config diagnostics 'voipdiagnostics'
	option enabled '0'
	option path '/tmp'
	option filename 'voip_diagnostics.txt'
	option action 'idle'
        option log_level 'low'

#/*******************************************************************/
#/*                   features configuration                         */
#/*******************************************************************/
config features 'features'
	list feature ''

#/*******************************************************************/
#/*                   network configuration                         */
#/*******************************************************************/
config network 'sip_net'
	option config 'mmpbxrvsipnet'

config network 'internal_net'
	option config 'mmpbxinternalnet'

#/*******************************************************************/
#/*                    device configuration                         */
#/*******************************************************************/
config device 'fxs_dev_0'
	option config 'mmpbxbrcmfxsdev'

config device 'fxs_dev_1'
	option config 'mmpbxbrcmfxsdev'

#/*******************************************************************/
#/*                   profile configuration                         */
#/*******************************************************************/
config profile 'internal_profile_0'
	option config 'mmpbxinternalnet'

config profile 'internal_profile_1'
	option config 'mmpbxinternalnet'

config profile 'internal_profile_2'
	option config 'mmpbxinternalnet'

config profile 'sip_profile_0'
	option config 'mmpbxrvsipnet'

config profile 'sip_profile_1'
	option config 'mmpbxrvsipnet'

config profile 'sip_profile_2'
	option config 'mmpbxrvsipnet'

#/*******************************************************************/
#/*                   outgoing maps                                 */
#/*******************************************************************/

#/*******************************************************************/
#/*                   internal maps                                 */
#/*******************************************************************/
config internal_map 'internal_map_internal_profile_1'
	option profile 'internal_profile_1'
	option device 'fxs_dev_0'

config internal_map 'internal_map_internal_profile_2'
	option profile 'internal_profile_2'
	option device 'fxs_dev_1'

#/*******************************************************************/
#/*                   incoming maps                                 */
#/*******************************************************************/
config incoming_map 'incoming_map_internal_profile_0'
	option profile 'internal_profile_0'
	list device 'fxs_dev_0'
	list device 'fxs_dev_1'

config incoming_map 'incoming_map_internal_profile_1'
	option profile 'internal_profile_1'
	list device 'fxs_dev_0'

config incoming_map 'incoming_map_internal_profile_2'
	option profile 'internal_profile_2'
	list device 'fxs_dev_1'

#/*******************************************************************/
#/*                   services                                      */
#/*******************************************************************/
config service 'ACR'
	option type 'ACR'
	list profile 'sip_profile_0'
	list profile 'sip_profile_1'
	list profile 'sip_profile_2'
	option provisioned '0'
	option activated '0'

config service 'BARGE_IN'
	option type 'BARGE_IN'
	list profile 'sip_profile_0'
	list profile 'sip_profile_1'
	list profile 'sip_profile_2'
	option provisioned '0'
	option activated '0'

config service 'CALL_RETURN'
	option type 'CALL_RETURN'
	list profile 'sip_profile_0'
	list profile 'sip_profile_1'
	list profile 'sip_profile_2'
	option provisioned '0'
	option activated '0'

config service 'CALL_WAITING'
	option type 'CALL_WAITING'
	list device 'fxs_dev_0'
	list device 'fxs_dev_1'
	option provisioned '1'
	option activated '1'
	option timeout '1200'
	option cw_reject_persistency '0'

config service 'CFBS'
	option type 'CFBS'
	list profile 'sip_profile_0'
	list profile 'sip_profile_1'
	list profile 'sip_profile_2'
	option provisioned '0'
	option activated '0'

