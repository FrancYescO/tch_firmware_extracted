	list profile 'sip_profile_0'
	list profile 'sip_profile_1'
	list profile 'sip_profile_2'
	option provisioned '0'
	option activated '0'

config service 'CLIP'
	option type 'CLIP'
	list profile 'internal_profile_0'
	list profile 'internal_profile_1'
	list profile 'internal_profile_2'
	list profile 'sip_profile_0'
	list profile 'sip_profile_1'
	list profile 'sip_profile_2'
	option provisioned '1'
	option activated '1'

config service 'CLIR'
	option type 'CLIR'
	list profile 'internal_profile_1'
	list profile 'internal_profile_2'
	list profile 'sip_profile_0'
	list profile 'sip_profile_1'
	list profile 'sip_profile_2'
	option provisioned '0'
	option activated '0'

config service 'CONFERENCE_dev'
	option type 'CONFERENCE'
	list device 'fxs_dev_0'
	list device 'fxs_dev_1'
	option provisioned '1'
	option activated '1'

config service 'CONFERENCE_prof'
	option type 'CONFERENCE'
	list profile 'sip_profile_0'
	list profile 'sip_profile_1'
	list profile 'sip_profile_2'
	option provisioned '1'
	option activated '1'

config service 'HOLD'
	option type 'HOLD'
	list profile 'internal_profile_0'
	list profile 'internal_profile_1'
	list profile 'internal_profile_2'
	list profile 'builtin_internal_profile'
	list profile 'sip_profile_0'
	list profile 'sip_profile_1'
	list profile 'sip_profile_2'
	option provisioned '1'
	option activated '1'

config service 'MWI'
	option type 'MWI'
	list profile 'sip_profile_0'
	list profile 'sip_profile_1'
	list profile 'sip_profile_2'
	option provisioned '1'
	option activated '1'

config service 'TRANSFER'
	option type 'TRANSFER'
	list device 'fxs_dev_0'
	list device 'fxs_dev_1'
	option provisioned '0'
	option activated '0'
	option internal_transfer '1'
	option external_transfer '1'

config service 'WARMLINE_0'
	option type 'WARMLINE'
	list device 'fxs_dev_0'
	option provisioned '0'
	option activated '0'
	option timeout '10'
	option destination ''

config service  'WARMLINE_1'
	option type 'WARMLINE'
	list device 'fxs_dev_1'
	option provisioned '0'
	option activated '0'
	option timeout '10'
	option destination ''


#/*******************************************************************/
#/*                   SCC                                           */
#/*******************************************************************/
config scc 'scc_generic'
	list network 'sip_net'
	option eon '#'
	option separator '*'
	option inter_digit_timer '3000'
	option mode 'standard'

#/*******************************************************************/
#/*                   dial plan                                     */
#/*******************************************************************/
config dial_plan 'dial_plan_generic'
	list network 'sip_net'
	option eon '#'
	option closed_inter_digit_timer '3000'
	option open_inter_digit_timer '3000'
	option no_match_action 'allow'
	option selection_mode 'legacy'
	option syntax 'pcre_with_translations'
	option no_match_include_eon '1'

config dial_plan_entry
	option dial_plan 'dial_plan_generic'
	option pattern '^*21*'
	option min_length '5'
	option max_length '30'
	option enabled '1'
	option allow '1'
	option include_eon '1'
	option priority 'low'
	option data '0'
	option apply_forced_profile '0'
	option position_of_modify '0'
	option remove_number_of_characters '0'

