{
  "package" : "com.netdumasoftware.neighwatch",
  "version" : "0.0.1",
  "foreground" : false,
  "background" : true,
  "system": true,
  "bind" : [
      { 
        "func" : "on_l3_map_l2",
        "package" : "com.netdumasoftware.arpwatch",
        "method" : "map"
      },
      {
        "func" : "on_l3_unmap",
        "package" : "com.netdumasoftware.arpwatch",
        "method" : "unmap"
      },
      {
        "func" : "on_neigh_new",
        "package" : "com.netdumasoftware.neighwatch",
        "method" : "newneigh"
      },
      {
        "func" : "on_neigh_dup",
        "package" : "com.netdumasoftware.neighwatch",
        "method" : "dupneigh"
      },
      {
        "func" : "on_neigh_del",
        "package" : "com.netdumasoftware.neighwatch",
        "method" : "delneigh"
      },
      {
        "func" : "on_init",
        "package" : "com.netdumasoftware.api",
        "method" : "init"
      }
  ],
  "methods" : [
    {
      "func" : "dhcp_event",
      "method" : "dhcp_event",
      "arguments" : {
        "event" : "string",
        "mac" : "string",
        "ip" : "string",
        "hostname" : "string"
      }
    },
    {
      "func" : "get_interface_info",
      "method" : "get_interface_info",
      "arguments" : {
        "mac" : "string"
      }
    },
    {
      "func" : "get_arp_table",
      "method" : "get_arp_table",
      "arguments" : {}
    }
  ]
}
