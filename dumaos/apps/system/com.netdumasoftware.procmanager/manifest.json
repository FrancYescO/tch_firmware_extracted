{
  "package" : "com.netdumasoftware.procmanager",
  "version" : "0.0.1",
  "name" : "Process Manager",
  "background" : true,
  "foreground" : false,
  "system": true,
  "bind" : [
      {
        "func" : "on_init",
        "package" : "com.netdumasoftware.api",
        "method" : "init"
      },
      {
        "func" : "on_cleanup",
        "package" : "com.netdumasoftware.api",
        "method" : "cleanup"
      }
  ],
  "methods" : [
    {
      "func" : "licence_permitted",
      "method" : "licence_permitted",
      "arguments" : {
        "rappid" : "string",
        "featureid" : "string"
      }
    },
    {
      "func" : "reload_installed",
      "method" : "reload_installed",
      "arguments" : {}
    },
    {
      "func" : "on_prepare_language",
      "method" : "prepare_language",
      "arguments" : {}
    }
  ]
}
