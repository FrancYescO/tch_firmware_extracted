  if [ -n "$map_agent_bh_list" ]; then
      procd_append_param env MAP_AGENT_BH_LIST=$map_agent_bh_list
  fi

  config_get val controller_selection enabled
  if [ -n "$val" ]; then
      procd_append_param env MAP_AGENT_EMCS_AUTOSELECTION_ENABLED="$val"
  fi

