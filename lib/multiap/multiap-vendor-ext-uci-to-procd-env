config_load multiap

config_get val vendor_extensions enabled
if [ -n "$val" ]; then
  procd_append_param env MAP_VENDOR_EXTENSIONS_ENABLED="$val"
fi

config_get val vendor_extensions feature_extension
if [ -n "$val" ]; then
  procd_append_param env MAP_SUPPORTED_FEATURE_EXTENSIONS="$val"
fi

config_get val profile3_extensions enabled
if [ -n "$val" ]; then
  procd_append_param env MAP_PROFILE3_EXTENSIONS_ENABLED="$val"
fi

config_get val vendor_extensions vendor_oui
if [ -n "$val" ]; then
  procd_append_param env MAP_VENDOR_EXTENSION_OUI="$val"
fi

config_load env

config_get val rip eth_mac
if [ -n "$val" ]; then
  procd_append_param env MAP_BR_LAN_MAC_ADDR="$val"
fi

config_get prod_class var prod_name
if [ -n "$prod_class" ]; then
    procd_append_param env MAP_AGENT_AIRTIES_PROD_CLASS=$prod_class
fi

config_load airties_edge

config_get airties_client_id cloud_comm client_id
if [ -n "$airties_client_id" ]; then
      procd_append_param env MAP_AGENT_AIRTIES_CLIENT_ID="$airties_client_id"
fi

