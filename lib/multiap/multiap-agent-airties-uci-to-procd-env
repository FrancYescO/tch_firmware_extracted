
## Get Airties Config
  airties_client_id=`uci -c "$CONTAINER_CONFIG_DIR" get airties_edge.cloud_comm.client_id`
  if [ -n "$airties_client_id" ]; then
      procd_append_param env MAP_AGENT_AIRTIES_CLIENT_ID="$airties_client_id"
  fi

  airties_client_secret=`uci -c "$CONTAINER_CONFIG_DIR" get airties_edge.cloud_comm.client_password`
  if [ -n "$airties_client_secret" ]; then
      procd_append_param env MAP_AGENT_AIRTIES_CLIENT_SECRET="$airties_client_secret"
  fi

