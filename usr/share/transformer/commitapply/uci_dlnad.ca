^dlnad%..+%.enabled /etc/init.d/dlnad stop && /etc/init.d/mud stop && /etc/init.d/mvfs stop && /etc/init.d/mvfs start && /etc/init.d/mud start && /etc/init.d/dlnad start
^dlnad%..+%.friendly_name /etc/init.d/dlnad reload
^dlnad%..+%.mvfs_logLevel /etc/init.d/mvfs restart && /etc/init.d/mud restart
^dlnad%..+%.mud_logLevel /etc/init.d/mud restart
^dlnad%..+%.share_all_folder /etc/init.d/mud reload
^dlnad%..+%.sharing /etc/init.d/mud reload
^dlnad%..+%.max_sharedfolders /etc/init.d/mud reload
^dlnad%..+%.share_path /etc/init.d/mud reload
^dlnad%..+%.dynamic /etc/init.d/mud stop && /etc/init.d/mvfs stop && /etc/init.d/mvfs start && /etc/init.d/mud start
