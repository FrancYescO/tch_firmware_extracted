^firewall /etc/init.d/firewall reload ;
^dmz_disable /usr/share/transformer/scripts/delete_dmz.sh ;
^deleteForwardedPorts echo "0" > /proc/sys/net/netfilter/nf_conntrack_tcp_loose ; read portNum < /tmp/.port ; conntrack -D -p tcp --dport $portNum ; rm /tmp/.port ;
^firewall%.[^%.]+%.pinholerule[^%.]+%.dest_mac ubus call pinholehelper update ;
^flushicmpv4 /etc/init.d/firewall reload ; conntrack -D -p icmp ;
^flushicmpv6 /etc/init.d/firewall reload ; conntrack -D -f ipv6 -p icmpv6 ;
