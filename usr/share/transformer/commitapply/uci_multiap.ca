^multiap%..+%.cred%d+%. ubus send multiap.controller_credentials '{}'
^multiap%..*%.cred1%.state /usr/share/transformer/scripts/wifi_tod.lua
^multiap%..*%.controller%.enabled /usr/share/transformer/scripts/copy_wireless_config_to_multiap.lua; /etc/init.d/multiap_controller restart &
^multiap%..*%.agent%.enabled /usr/bin/lua -e "uc=require('transformer.shared.bandsteerhelper') uc.disableBandsteer('agent')"; /etc/init.d/multiap_agent restart &
